-- 2.4.2 zxAnMldLogServerTable
------------------------------------------------------------------------------

    zxAnMldLogServerTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnMldLogServerEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "MLD log servers table."
        ::= { zxAnMldLogObjects 2 }

    zxAnMldLogServerEntry OBJECT-TYPE
        SYNTAX ZxAnMldLogServerEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
           "MLD log servers entry."
        INDEX   {
            zxAnMldLogServerIpAddrType,
            zxAnMldLogServerIpAddr
        }
        ::= { zxAnMldLogServerTable 1 }

    ZxAnMldLogServerEntry  ::= SEQUENCE {
        zxAnMldLogServerIpAddrType     InetAddressType,
        zxAnMldLogServerIpAddr         InetAddress,
        zxAnMldLogServerUserName       DisplayString,
        zxAnMldLogServerPassword       DisplayString,
        zxAnMldLogServerFtpType        INTEGER,
        zxAnMldLogVrfName              DisplayString,
        zxAnMldLogHostIpAddrType       InetAddressType,
        zxAnMldLogHostIpAddr           InetAddress,
        zxAnMldLogServerRowStatus      RowStatus
    }
    
    zxAnMldLogServerIpAddrType  OBJECT-TYPE
        SYNTAX      InetAddressType
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "MLD log server IP address type."
        DEFVAL { ipv4 }
        ::= { zxAnMldLogServerEntry 1 }

    zxAnMldLogServerIpAddr  OBJECT-TYPE
        SYNTAX      InetAddress
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "MLD log server IP address."
        ::= { zxAnMldLogServerEntry 2 }
        
    zxAnMldLogServerUserName  OBJECT-TYPE
        SYNTAX      DisplayString(SIZE(1..32))
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION  
            "MLD log server user name."
        ::= { zxAnMldLogServerEntry 3 }
    
    zxAnMldLogServerPassword  OBJECT-TYPE
        SYNTAX      DisplayString(SIZE(0..32))
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION  
            "MLD log server password."
        ::= { zxAnMldLogServerEntry 4 }
        
    zxAnMldLogServerFtpType  OBJECT-TYPE
        SYNTAX      INTEGER {
            ftp(1),
            sftp(2)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION  
            "MLD log server protocol type."
        DEFVAL { ftp }
        ::= { zxAnMldLogServerEntry 5 }    
        
    zxAnMldLogVrfName  OBJECT-TYPE
        SYNTAX      DisplayString(SIZE(0..32)) 
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION 
            "The VRF name.
             When set this variable, an empty string is not supported.
             When get this variable, always return an empty string."
        ::= { zxAnMldLogServerEntry 6 }

    zxAnMldLogHostIpAddrType  OBJECT-TYPE
        SYNTAX      InetAddressType
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "Host IP address type."
        DEFVAL { ipv4 }
        ::= { zxAnMldLogServerEntry 7 }
        
    zxAnMldLogHostIpAddr  OBJECT-TYPE
        SYNTAX      InetAddress
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "Host IP address."
        ::= { zxAnMldLogServerEntry 8 }
        
    zxAnMldLogServerRowStatus  OBJECT-TYPE 
        SYNTAX           RowStatus
        MAX-ACCESS       read-create
        STATUS           current
        DESCRIPTION      
            "The status of this conceptual row.
            
            The supported actions of this conceptual row only include:
            'createAndGo' and 'destroy'.

            To create a row in this table, a manager must set this object to 
            createAndGo(4), and must comply with the multi-variables binding 
            rule described in zxAnMldLogServerEntry.
    
            To delete a row in this table, a manager must set this object to 
            destroy(6). 
             
            To get this object, the agent always returns active(1)."
        ::=  { zxAnMldLogServerEntry  50 }
        
