-- 2.50.2 zxAnMldPktStatsTable
------------------------------------------------------------------------------
    zxAnMldPktStatsTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnMldPktStatsEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "A table that contains MLD packet statistics information."
        ::= { zxAnMldStatsObjects 2 }

    zxAnMldPktStatsEntry OBJECT-TYPE
        SYNTAX      ZxAnMldPktStatsEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "When query MLD global statistics, all index must be zero.            

            When query MLD statistics of a specific MVLAN, ifIndex and 
            zxAnSubIfIndex must be zero.

            When query MLD statistics of a specific interface, zxAnMldMVid 
            must be zero.

            When query MLD statistics of a specific interface in a MVLAN,
            all index must be non zero."
        INDEX { zxAnMldMVid, ifIndex, zxAnSubIfIndex }
        ::= { zxAnMldPktStatsTable 1 }

    ZxAnMldPktStatsEntry ::=
        SEQUENCE {
            zxAnMldPktStatsRxCommQry        Counter32,
            zxAnMldPktStatsRxSpecialQry     Counter32,
            zxAnMldPktStatsRxV1Report       Counter32,
            zxAnMldPktStatsRxV2Report       Counter32,
            zxAnMldPktStatsRxLeave          Counter32,
            zxAnMldPktStatsRxError          Counter32,
            zxAnMldPktStatsRxUnknown        Counter32,
            zxAnMldPktStatsTxCommQry        Counter32,
            zxAnMldPktStatsTxSpecialQry     Counter32,
            zxAnMldPktStatsTxV1Report       Counter32,
            zxAnMldPktStatsTxV2Report       Counter32,
            zxAnMldPktStatsTxLeave          Counter32,
            zxAnMldPktStatsDropRxCommQry    Counter32,
            zxAnMldPktStatsDropRxSpeQry     Counter32,
            zxAnMldPktStatsDropRxV1Report   Counter32,
            zxAnMldPktStatsDropRxV2Report   Counter32,
            zxAnMldPktStatsDropRxLeave      Counter32,
            zxAnMldPktStatsDropRxUnknown    Counter32,
            zxAnMldPktStatsJoinSuccess      Counter32,
            zxAnMldPktStatsJoinFailure      Counter32,
            zxAnMldPktStatsReset            Integer32
        }

