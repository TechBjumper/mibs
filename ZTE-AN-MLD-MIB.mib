    Counter32,TimeTicks                                 FROM SNMPv2-SMI
    DisplayString,MacAddress,RowStatus                  FROM SNMPv2-TC
    OBJECT-GROUP,MODULE-COMPLIANCE                      FROM SNMPv2-CONF
    InetAddressIPv6,InetAddress                         FROM INET-ADDRESS-MIB
    ZxAnPortList,VlanId                                 FROM ZTE-AN-TC-MIB
    ifIndex                                             FROM IF-MIB
    zxAnSubIfIndex                                      FROM ZTE-AN-IF-MIB
    InetAddress,InetAddressType                         FROM INET-ADDRESS-MIB
    zxAnMulticast                                       FROM ZTE-AN-SMI;

    zxAnMldMib MODULE-IDENTITY
        LAST-UPDATED    "201112270900Z"
        ORGANIZATION    "ZTE Corporation"
        CONTACT-INFO    "Weng Ningning  ZTE Corporation
                         Mail: wen.ningning@zte.com.cn
                         Tel : +86-21-68897487"
        DESCRIPTION     "This MIB defines Description of zte mld service 
                         objects."
        REVISION        "201112270900Z"
        DESCRIPTION     "Initial version of this MIB module."
        ::= {zxAnMulticast 2}

    zxAnMldGlobalObjects       OBJECT IDENTIFIER ::= {zxAnMldMib 1}
    zxAnMldObjects             OBJECT IDENTIFIER ::= {zxAnMldMib 2}
    zxAnMldConformance         OBJECT IDENTIFIER ::= {zxAnMldMib 4}

    zxAnMldProtocolObjects     OBJECT IDENTIFIER ::= {zxAnMldObjects 1}
    zxAnMldMgmtObject          OBJECT IDENTIFIER ::= {zxAnMldObjects 2}
    zxAnMldVplsObjects         OBJECT IDENTIFIER ::= {zxAnMldObjects 3}
    zxAnMldLogObjects          OBJECT IDENTIFIER ::= {zxAnMldObjects 4}
    zxAnMldStatsObjects        OBJECT IDENTIFIER ::= {zxAnMldObjects 50}

    zxAnMldProtoBaseObjects    OBJECT IDENTIFIER ::= {zxAnMldProtocolObjects 2}
    zxAnMldSnoopingObjects     OBJECT IDENTIFIER ::= {zxAnMldProtocolObjects 3}
    zxAnMldProxyObjects        OBJECT IDENTIFIER ::= {zxAnMldProtocolObjects 4}

    zxAnMldProtoGlobalObjects  OBJECT IDENTIFIER ::= {zxAnMldProtoBaseObjects 1}

    zxAnMldMgmtGlobalObject    OBJECT IDENTIFIER ::= {zxAnMldMgmtObject 1}
    
    zxAnMldLogGlobalObjects    OBJECT IDENTIFIER ::= {zxAnMldLogObjects 1}

