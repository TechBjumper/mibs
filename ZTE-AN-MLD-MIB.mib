-- 4. zxAnMldConformance
--------------------------------------------------------------------------------
zxAnMldCompliances      OBJECT IDENTIFIER ::= {zxAnMldConformance 1}
zxAnMldGroups           OBJECT IDENTIFIER ::= {zxAnMldConformance 2}

    zxAnMldCompliance MODULE-COMPLIANCE
        STATUS current
        DESCRIPTION
            "The compliance statement for entities which implement the
             ZTE-AN-MLD-MIB."
        MODULE -- this module
            MANDATORY-GROUPS {
                zxAnMldProtocolGroup,
                zxAnMldManagementGroup,
                zxAnMldStatsGroup
            }
        ::= { zxAnMldCompliances 1 }

    zxAnMldProtocolGroup OBJECT-GROUP
        OBJECTS {
            zxAnMldEnable, zxAnMldIfAdminStatus,
            zxAnMldIfProtoVersion, zxAnMldIfFastLeaveEnable,
            zxAnMldMVlanMldAdminStatus, zxAnMldMVlanMldV1OperMode,
            zxAnMldMVlanMldV2OperMode, zxAnMldMVlanMldWorkMode,
            zxAnMldMVlanMldSsmCtrlMode, zxAnMldMVlanMldSsmIpAddr,
            zxAnMldMVlanMldSsmIpAddrMask, zxAnMldSnoopingMVlanAgingTime,
            zxAnMldMVlanRobustnessVariable, zxAnMldMVlanHostVersion,
            zxAnMldMVlanHostIpAddr, zxAnMldMVlanUnsolReportInterval,
            zxAnMldMVlanHostQuerier, zxAnMldMVlanHostActualVersion,
            zxAnMldMVlanHostV1QuerierTimer, zxAnMldMVlanRouterIpAddr,
            zxAnMldMVlanQueryInterval, zxAnMldMVlanQueryRespInterval,
            zxAnMldMVlanLastMemberQryIntvl, zxAnMldMVlanLastMemberQryCount,
            zxAnMldMVlanStartUpQryInterval, zxAnMldMVlanStartUpQryCount,
            zxAnMldMVlanMldPacketPriority,zxAnMldMVlanQryPktProtoVersion,
            zxAnMldMVlanMldPktTrafficClass
        }
        STATUS current
        DESCRIPTION
            "A collection of objects providing Mld protocol information."
        ::= { zxAnMldGroups 1 }

    zxAnMldManagementGroup OBJECT-GROUP
        OBJECTS {
            zxAnMldUserTrackEnable, zxAnMldBandwidthCtrlEnable,
            zxAnMldGroupPrejoinInterval, zxAnMldSpanVlanEnable,
            zxAnMldMVlanGroupFilterEnable, zxAnMldMVlanRowStatus,
            zxAnMldMgmtMVlanRecvIfRowStatus, zxAnMldMVlanSrcIfRowStatus,
            zxAnMldGroupFilterRowStatus,
            zxAnMldGroupStaticIfRowStatus,
            zxAnMldUserIpAddr, 
            zxAnMldGrpUserSrcFilterMode, zxAnMldGroupPrejoinRowStatus, 
            zxAnMldGroupBandwidth, zxAnMldGroupConfRowStatus,
            zxAnMldSpanVlanRowStatus
        }
        STATUS current
        DESCRIPTION
            "A collection of objects providing Mld management information."
        ::= { zxAnMldGroups 2 }

    zxAnMldStatsGroup OBJECT-GROUP
        OBJECTS {
            zxAnMldPktStatsRxCommQry, zxAnMldPktStatsRxSpecialQry,
            zxAnMldPktStatsRxV1Report, zxAnMldPktStatsRxV2Report,
            zxAnMldPktStatsRxLeave, zxAnMldPktStatsRxError,
            zxAnMldPktStatsRxUnknown, zxAnMldPktStatsTxCommQry,
            zxAnMldPktStatsTxSpecialQry, zxAnMldPktStatsTxV1Report,
            zxAnMldPktStatsTxV2Report, zxAnMldPktStatsTxLeave,
            zxAnMldPktStatsDropRxCommQry, zxAnMldPktStatsDropRxSpeQry,
            zxAnMldPktStatsDropRxV1Report, zxAnMldPktStatsDropRxV2Report,
            zxAnMldPktStatsDropRxLeave, zxAnMldPktStatsDropRxUnknown,
            zxAnMldPktStatsJoinSuccess, zxAnMldPktStatsJoinFailure,
            zxAnMldPktStatsReset, zxAnMldIfCurrActGroups,
            zxAnMldMVlanCurrActGroups, zxAnMldGroupActiveUsers
        }
        STATUS current
        DESCRIPTION
            "A collection of objects providing Mld statistics information."
        ::= { zxAnMldGroups 4 }

END
