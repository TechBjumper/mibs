------------------------------------------------------------------------------
    zxAnMldLogEnable OBJECT-TYPE
        SYNTAX      INTEGER {
                        enabled(1),
                        disabled(2)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The administrative status of MLD log."
        DEFVAL { disabled }
        ::= { zxAnMldGlobalObjects 1 }
        
    zxAnMldRouterAlertEnable OBJECT-TYPE
        SYNTAX      INTEGER {
            enabled(1),
            disabled(2)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The administrative status of MLD router alert option."
        DEFVAL { enabled }
        ::= { zxAnMldGlobalObjects 2 }
        
    zxAnMldBackwardCompatibilityEn OBJECT-TYPE
        SYNTAX      INTEGER {
            enabled(1),
            disabled(2)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The administrative status of MLD backward compatibility."
        DEFVAL { disabled }
        ::= { zxAnMldGlobalObjects 3 } 

    zxAnMldSyslogReportingEnable OBJECT-TYPE
        SYNTAX      INTEGER {
            enabled(1),
            disabled(2)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The administrative status of MLD syslog reporting."
        DEFVAL { disabled }
        ::= { zxAnMldGlobalObjects 4 }

    zxAnMldCapabilities OBJECT-TYPE
        SYNTAX      BITS {
            notSupportIfBandwidth(0),
            supportMVlanPortListIITable(1)
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The MLD capabilities.
             The bit value 1 means the function represented by this bit is 
             supported by this equipment.
             bit 0: value 1 means not supporting zxAnMldIfBandwidth. 
             bit 1: value 1 means supporting zxAnMldMgmtMVlanPortListIITable and
                    not supporting zxAnMldMgmtMVlanPortListTable.
             NOTE: No recommended to display it in NMS." 
        ::= { zxAnMldGlobalObjects  100 }
      
------------------------------------------------------------------------------
-- 2.1.2.1 zxAnMldProtoGlobalObjects
------------------------------------------------------------------------------
    zxAnMldEnable OBJECT-TYPE
        SYNTAX      INTEGER {
                        enabled(1),
                        disabled(2)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The administrative status of global MLD service."
        DEFVAL { disabled }
        ::= { zxAnMldProtoGlobalObjects 1 }

------------------------------------------------------------------------------
-- 2.1.2.2 zxAnMldProtoIfTable
------------------------------------------------------------------------------
    zxAnMldProtoIfTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnMldProtoIfEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "A table that contains all MLD protocol information of an user 
            interface."
        ::= { zxAnMldProtoBaseObjects 2 }

    zxAnMldProtoIfEntry OBJECT-TYPE
        SYNTAX      ZxAnMldProtoIfEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
           "A list of entries of zxAnMldProtoIfTable."
        INDEX { ifIndex, zxAnSubIfIndex }
        ::= { zxAnMldProtoIfTable 1 }

    ZxAnMldProtoIfEntry ::=
        SEQUENCE {
            zxAnMldIfAdminStatus          INTEGER,
            zxAnMldIfProtoVersion         INTEGER,
            zxAnMldIfFastLeaveEnable      INTEGER,
            zxAnMldIfBandwidth            Integer32,
            zxAnMldIfMaxGroups            Integer32
        }

    zxAnMldIfAdminStatus OBJECT-TYPE
        SYNTAX      INTEGER {
                        enabled(1),
                        drop(3),
                        notSupport(255)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The administrative status of port's MLD service.
             The value 255 means the object is not supported."
        DEFVAL { enabled }
        ::= { zxAnMldProtoIfEntry 1 }

    zxAnMldIfProtoVersion OBJECT-TYPE
        SYNTAX      INTEGER {
                        mldv1(1),
                        mldv2(2),
                        notSupport(255)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The max version of MLD protocol which is supported on this port.
             The value 255 means the object is not supported."
        DEFVAL { mldv2 }
        ::= { zxAnMldProtoIfEntry 2 }

    zxAnMldIfFastLeaveEnable OBJECT-TYPE
        SYNTAX      INTEGER {
                        enabled(1),
                        disabled(2),
                        notSupport(255)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The administrative status of port's fast-leave property.
             The value 255 means the object is not supported."
        REFERENCE
            "TR-101 Page 13"
        DEFVAL { enabled }
        ::= { zxAnMldProtoIfEntry 3 }

    zxAnMldIfBandwidth OBJECT-TYPE
        SYNTAX      Integer32 (100..10240000|2147483647)
        UNITS       "kbps"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Multicast bandwidth of this port.
             Uplink port/PON port: The valid range is 10240~10240000 kbps, and 
             the default value is 1024000 kbps.
             VPort: The valid range is 100~204800 kbps, and the default value 
             is 102400 kbps.
             The value 2147483647 means the object is not supported."
        ::= { zxAnMldProtoIfEntry 4 }
        
    zxAnMldIfMaxGroups OBJECT-TYPE
        SYNTAX      Integer32 (0..8192|2147483647)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Maximum multicast groups that the port may join. 
             The value 0 means the port cannot join any groups.
             The value 2147483647 means the object is not supported."
        DEFVAL { 8192 }
        ::= { zxAnMldProtoIfEntry 5 }
------------------------------------------------------------------------------
-- 2.1.2.3 zxAnMldProtoMVlanTable
------------------------------------------------------------------------------
    zxAnMldProtoMVlanTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnMldProtoMVlanEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "A table that contains MLD protocol information in Multicast
            VLAN."
        ::= { zxAnMldProtoBaseObjects 3 }

    zxAnMldProtoMVlanEntry OBJECT-TYPE
        SYNTAX      ZxAnMldProtoMVlanEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "A list of entries of zxAnMldProtoMVlanTable."
        INDEX { zxAnMldMVid }
        ::= { zxAnMldProtoMVlanTable 1 }

    ZxAnMldProtoMVlanEntry ::=
        SEQUENCE {
            zxAnMldMVid                       Integer32,
            zxAnMldMVlanMldAdminStatus        INTEGER,
            zxAnMldMVlanMldV1OperMode         INTEGER,
            zxAnMldMVlanMldV2OperMode         INTEGER,
            zxAnMldMVlanMldWorkMode           INTEGER,
            zxAnMldMVlanMldSsmCtrlMode        INTEGER,
            zxAnMldMVlanMldSsmIpAddr          InetAddressIPv6,
            zxAnMldMVlanMldSsmIpAddrMask      InetAddressIPv6,
            zxAnMldMVlanMldCVid               Integer32,
            zxAnMldMvlanMldMaxGroups          Integer32,
            zxAnMldMVlanGenQryPktTxGrpMode    INTEGER
        }

    zxAnMldMVid OBJECT-TYPE
        SYNTAX      Integer32(1..4094)
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "Multicast VLAN ID."
        ::= { zxAnMldProtoMVlanEntry 1 }

    zxAnMldMVlanMldAdminStatus OBJECT-TYPE
        SYNTAX      INTEGER {
                        enabled(1),
                        drop(3)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The administrative status of MLD service in this MVLAN."
        DEFVAL { enabled }
        ::= { zxAnMldProtoMVlanEntry 2 }

    zxAnMldMVlanMldV1OperMode OBJECT-TYPE
        SYNTAX      INTEGER {
                        accept(1),
                        drop(3)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The operational mode of MLD v1 packet."
        DEFVAL { accept }
        ::= { zxAnMldProtoMVlanEntry 3 }

    zxAnMldMVlanMldV2OperMode OBJECT-TYPE
        SYNTAX      INTEGER {
                        accept(1),
                        drop(3)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The operational mode of MLD v2 packet."
        DEFVAL { accept }
        ::= { zxAnMldProtoMVlanEntry 4 }

    zxAnMldMVlanMldWorkMode OBJECT-TYPE
        SYNTAX      INTEGER {
                        snooping(1),
                        spr(2),
                        proxy(3),
                        router(4)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The MLD work mode in this MVLAN. The meanings of the values
            are:
                snooping(1) - transparently forward MLD packet, snooping
                    is defined in RFC 4541 and TR-101.
                spr(2) - transparently forward MLD general query and
                    suppress MLD report/leave for existing group membership,
                    SPR(Snooping with Proxy Reporting) is defined in TR-101.
                proxy(3) - act as a host on the V interface and act as a
                    router on the U interface, proxy is defined in RFC4605.
                router(4) - act as a terminal on the V interface and act as a
                    router on the U interface, router is defined in TR-101."
        REFERENCE
            "RFC 4604, TR-101 Page 14"
        DEFVAL { proxy }
        ::= { zxAnMldProtoMVlanEntry 5 }

    zxAnMldMVlanMldSsmCtrlMode OBJECT-TYPE
        SYNTAX      INTEGER {
                        asm(1),
                        ssm(2),
                        asmAndSsm(3)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The ASM/SSM service control model. The meanings of the values
            are:
                asm(1) - only ASM(*, G) requests are permitted, ASM(Any-
                    Source Multicast) is defined in RFC 1112.
                ssm(2) - only SSM(S, G) requests are permitted, SSM(Source-
                    Specific Multicast) is defined in RFC 3569.
                asmAndSsm(3) --- both ASM and SSM requests are permitted."
        REFERENCE
            "RFC 4604, RFC 3376"
        DEFVAL { asmAndSsm }
        ::= { zxAnMldProtoMVlanEntry 6 }

    zxAnMldMVlanMldSsmIpAddr OBJECT-TYPE
        SYNTAX      InetAddressIPv6
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The SSM IP address in this MVLAN.
             All SSM(S, G) request must be in the same IP network segment as
             the configued SSM IP address and netmask."
        REFERENCE
            "RFC 4604"
        DEFVAL { 'FF300000000000000000000000000000'H }
        ::= { zxAnMldProtoMVlanEntry 7 }

    zxAnMldMVlanMldSsmIpAddrMask OBJECT-TYPE
        SYNTAX      InetAddressIPv6
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The mask of SSM IP address in this MVLAN.
             All SSM(S, G) request must be in the same IP network segment as
             the configued SSM IP address and netmask."
        REFERENCE
            "RFC 4604"
        DEFVAL { 'FFF0FFFFFFFFFFFFFFFFFFFF00000000'H }
        ::= { zxAnMldProtoMVlanEntry 8 }
    
    zxAnMldMVlanMldCVid OBJECT-TYPE
        SYNTAX      Integer32(0..4094)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Multicast C-VID.
             Value 0 means no C-VID tag."
        DEFVAL { 0 }
        ::= { zxAnMldProtoMVlanEntry 9 }

    zxAnMldMvlanMldMaxGroups OBJECT-TYPE
        SYNTAX      Integer32 (0..8192)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Maximum multicast groups that the MVLAN may join. The value 0 means 
             the MVLAN cannot join any groups."
        DEFVAL { 8192 }
        ::= { zxAnMldProtoMVlanEntry 10 }

    zxAnMldMVlanGenQryPktTxGrpMode OBJECT-TYPE
        SYNTAX      INTEGER {
            usersInGroup(1),
            allUsers(2)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The general query packets transmission group mode only on pon
             interfaces."
        DEFVAL { usersInGroup }
        ::= { zxAnMldProtoMVlanEntry 11 }

------------------------------------------------------------------------------
-- 2.1.3.2 zxAnMldSnoopingMVlanTable
------------------------------------------------------------------------------
    zxAnMldSnoopingMVlanTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnMldSnoopingMVlanEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "A table that contains MLD Snooping informations in multicast
            VLAN."
        ::= { zxAnMldSnoopingObjects 2 }

    zxAnMldSnoopingMVlanEntry OBJECT-TYPE
        SYNTAX      ZxAnMldSnoopingMVlanEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "A list of entries of zxAnMldSnoopingMVlanTable."
        INDEX { zxAnMldMVid }
        ::= { zxAnMldSnoopingMVlanTable 1 }

    ZxAnMldSnoopingMVlanEntry ::=
        SEQUENCE {
            zxAnMldSnoopingMVlanAgingTime      Integer32
        }

    zxAnMldSnoopingMVlanAgingTime OBJECT-TYPE
        SYNTAX      Integer32 (30..3600)
        UNITS       "seconds"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The timeout period in seconds for aging out multicast group
            memberships dynamically learned with MLD which operates on
            a per interface per VLAN per multicast group basis.
            Note that it only takes effect in snooping mode."
        DEFVAL { 300 }
        ::= { zxAnMldSnoopingMVlanEntry 1 }

------------------------------------------------------------------------------
-- 2.1.4.2 zxAnMldProxyMVlanTable
------------------------------------------------------------------------------
    zxAnMldProxyMVlanTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnMldProxyMVlanEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "A table that contains MLD proxy informations in Multicast
            VLAN."
        ::= { zxAnMldProxyObjects 2 }

    zxAnMldProxyMVlanEntry OBJECT-TYPE
        SYNTAX      ZxAnMldProxyMVlanEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "A list of entries of zxAnMldProxyMVlanTable."
        INDEX { zxAnMldMVid  }
        ::= { zxAnMldProxyMVlanTable 1 }

    ZxAnMldProxyMVlanEntry ::=
        SEQUENCE {
            zxAnMldMVlanRobustnessVariable     Integer32,
            zxAnMldMVlanHostVersion            INTEGER,
            zxAnMldMVlanHostIpAddr             InetAddressIPv6,
            zxAnMldMVlanUnsolReportInterval    Integer32,
            zxAnMldMVlanHostQuerier            InetAddressIPv6,
            zxAnMldMVlanHostActualVersion      INTEGER,
            zxAnMldMVlanHostV1QuerierTimer     TimeTicks,
            zxAnMldMVlanRouterIpAddr           InetAddressIPv6,
            zxAnMldMVlanQueryInterval          Integer32,
            zxAnMldMVlanQueryRespInterval      Integer32,
            zxAnMldMVlanLastMemberQryIntvl     Integer32,
            zxAnMldMVlanLastMemberQryCount     Integer32,
            zxAnMldMVlanStartUpQryInterval     Integer32,
            zxAnMldMVlanStartUpQryCount        Integer32,
            zxAnMldMVlanMldPacketPriority      Integer32,
            zxAnMldMVlanQryPktProtoVersion     INTEGER,
            zxAnMldMVlanMldPktTrafficClass     Integer32
        }

    zxAnMldMVlanRobustnessVariable OBJECT-TYPE
        SYNTAX      Integer32 (1..7)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "MLD robustness variable in the MVLAN."
        REFERENCE
            "RFC 3810 Section 9.1"
        DEFVAL { 2 }
        ::= { zxAnMldProxyMVlanEntry 1 }

    zxAnMldMVlanHostVersion OBJECT-TYPE
        SYNTAX      INTEGER {
                        auto(1),
                        mldv1(2),
                        mldv2(3)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The administrative version of MLD packet sent on the host
            interface.
                auto(1) - MLD host version is dynamicly determined by
                    the attached router version, according to MLD Host
                    Compatibility Mode described in RFC 3810.
                mldv1(2) - MLD host version is MLD v1.
                mldv2(3) - MLD host version is MLD v2."
        REFERENCE
            "RFC 3810 Section 8.2.1"
        DEFVAL { auto }
        ::= { zxAnMldProxyMVlanEntry 2 }

    zxAnMldMVlanHostIpAddr OBJECT-TYPE
        SYNTAX      InetAddressIPv6
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The source IP address of MLD packet sent on the host
            interface."
        DEFVAL { 'FE8000000000000000000000C0A8020E'H }
        ::= { zxAnMldProxyMVlanEntry 3 }

    zxAnMldMVlanUnsolReportInterval OBJECT-TYPE
        SYNTAX      Integer32 (1..60)
        UNITS       "seconds"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "MLD unsolicited report interval in the MVLAN."
        REFERENCE
            "RFC 3810 Section 9.11"
        DEFVAL { 1 }
        ::= { zxAnMldProxyMVlanEntry 4 }

    zxAnMldMVlanHostQuerier OBJECT-TYPE
        SYNTAX      InetAddressIPv6
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The IP address of the MLD Querier attached on the host
            interface which is learned from received MLD general query."
        REFERENCE
            "RFC 3810"
        ::= { zxAnMldProxyMVlanEntry 5 }

    zxAnMldMVlanHostActualVersion OBJECT-TYPE
        SYNTAX      INTEGER {
                        mldv1(1),
                        mldv2(2)
                    }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The running version of MLD packet sent on the host interface."
        REFERENCE
            "RFC 3810"
        DEFVAL { mldv2 }
        ::= { zxAnMldProxyMVlanEntry 6 }

    zxAnMldMVlanHostV1QuerierTimer OBJECT-TYPE
        SYNTAX      TimeTicks
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The time remaining until the host assumes that there are
            no MLDv1 routers present on the interface.  While this is
            non-zero, the host will reply to all queries with version 1
            membership reports.  This variable applies to MLDv2 hosts
            that are forced to run in v1 for compatibility with
            v1 routers present on the host interface."
        REFERENCE
            "RFC 3810 Section 8.2.1"
        DEFVAL { 0 }
        ::= { zxAnMldProxyMVlanEntry 7 }

     zxAnMldMVlanRouterIpAddr OBJECT-TYPE
        SYNTAX      InetAddressIPv6
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The source IP address of MLD query packet sent on the router
            interface."
        DEFVAL { 'FE8000000000000000000000C0A8020E'H }
        ::= { zxAnMldProxyMVlanEntry 8 }

    zxAnMldMVlanQueryInterval OBJECT-TYPE
        SYNTAX      Integer32 (30..300)
        UNITS       "seconds"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "MLD general query interval in the MVLAN."
        REFERENCE
            "RFC 3810 Section 9.2"
        DEFVAL { 125 }
        ::= { zxAnMldProxyMVlanEntry 9 }

    zxAnMldMVlanQueryRespInterval OBJECT-TYPE
        SYNTAX      Integer32 (1000..25000)
        UNITS       "millisecond"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "MLD query max response time in the MVLAN."
        REFERENCE
            "RFC 3810 Section 9.3"
        DEFVAL { 10000 }
        ::= { zxAnMldProxyMVlanEntry 10 }

    zxAnMldMVlanLastMemberQryIntvl OBJECT-TYPE
        SYNTAX      Integer32 (100..25500)
        UNITS       "millisecond"
        MAX-ACCESS  read-write
        STATUS current
        DESCRIPTION
            "MLD last member query interval in the MVLAN."
        REFERENCE
            "RFC 3810 Section 9.8"
        DEFVAL { 1000 }
        ::= { zxAnMldProxyMVlanEntry 11 }

    zxAnMldMVlanLastMemberQryCount OBJECT-TYPE
        SYNTAX      Integer32 (2..5)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "MLD last member query count in the MVLAN."
        REFERENCE
            "RFC 3810 Section 9.9"
        DEFVAL { 2 }
        ::= { zxAnMldProxyMVlanEntry 12 }

    zxAnMldMVlanStartUpQryInterval OBJECT-TYPE
        SYNTAX      Integer32 (1..100)
        UNITS       "seconds"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "MLD startup query interval in the MVLAN.
            When system is start up or port is up"
        REFERENCE
            "RFC 2710 Section 4, RFC 3810 Section 9.6"
        DEFVAL { 30 }
        ::= { zxAnMldProxyMVlanEntry 13 }

    zxAnMldMVlanStartUpQryCount OBJECT-TYPE
        SYNTAX      Integer32 (0..10)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "MLD startup query count in the MVLAN."
        REFERENCE
            "RFC 2710 Section 4, RFC 3810 Section 9.7"
        DEFVAL { 2 }
        ::= { zxAnMldProxyMVlanEntry 14 }

    zxAnMldMVlanMldPacketPriority OBJECT-TYPE
        SYNTAX      Integer32 (0..7)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
