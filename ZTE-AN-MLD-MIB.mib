        DEFVAL { disabled }
        ::= { zxAnMldMgmtGlobalObject 2 }
        
    zxAnMldBandwidthCtrlEnable OBJECT-TYPE
        SYNTAX      INTEGER {
            enabled(1),
            disabled(2)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Enable/disable the multicast bandwidth control.
             enabled(1): the multicast traffic on the user port will be checked 
                         before user attempts to join a new multicast group.
                         If the total multicast traffic on the user port has 
                         exceeded the configured rate of port, the user will 
                         not be allowed to join.
             disabled(2): the user may join any number of multicast groups 
                          eventhough the total multicast traffic on the user 
                          port has exceeded the configured rate of port."
        DEFVAL { disabled }
        ::= { zxAnMldMgmtGlobalObject 3 }
        
    zxAnMldGroupPrejoinInterval OBJECT-TYPE
        SYNTAX      Integer32 (60..300)
        UNITS       "Seconds"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The interval of MLD requests sent by the multicast group 
            which the prejoin is enabled."
        DEFVAL { 120 }
        ::= { zxAnMldMgmtGlobalObject 4 }
        
    zxAnMldSpanVlanEnable OBJECT-TYPE
        SYNTAX      INTEGER {
            enabled(1),
            disabled(2)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Enable/disable the Span-VLAN multicast. Multicast traffic will be 
            forwarded in all of VLANs if enabled."
        DEFVAL { enabled }
        ::= { zxAnMldMgmtGlobalObject 5 }
        
    zxAnMldGeneralQueryPktsTxMode OBJECT-TYPE
        SYNTAX      INTEGER {
            unicast(1),
            broadcast(2),
            both(3)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
