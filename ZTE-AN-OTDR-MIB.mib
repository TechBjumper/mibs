--  2.13 OTDR Line Detection Objects ------------------------------------------
-------------------------------------------------------------------------------
--   2.13.1 OTDR Detection Line Configuration Table
-------------------------------------------------------------------------------

    zxAnOtdrDetectionLineConfTable    OBJECT-TYPE
        SYNTAX          SEQUENCE OF ZxAnOtdrDetectionLineConfEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION     "OTDR detection line configuration table."
        ::= { zxAnOtdrDetectionLineObjects 2 }

    zxAnOtdrDetectionLineConfEntry    OBJECT-TYPE
        SYNTAX          ZxAnOtdrDetectionLineConfEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION     "OTDR detection line configuration entry."
        INDEX   { ifIndex, zxAnOtdrDetectLineId }
        ::= { zxAnOtdrDetectionLineConfTable 1 }

    ZxAnOtdrDetectionLineConfEntry  ::=  SEQUENCE {
        zxAnOtdrDetectLineId              Integer32,
        zxAnOtdrDetectLineLength          Integer32,
        zxAnOtdrDetectLineAlias           DisplayString,
        zxAnOtdrDetectLinePositionDiff    Integer32,
        zxAnOtdrDetectLineAbnPeakDiff     Integer32,
        zxAnOtdrDetectLineStatus          INTEGER
        }

    zxAnOtdrDetectLineId   OBJECT-TYPE
        SYNTAX          Integer32
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION     "OTDR detection line ID."
        ::= { zxAnOtdrDetectionLineConfEntry 1 }

    zxAnOtdrDetectLineLength   OBJECT-TYPE
        SYNTAX          Integer32(0..3000000)
        UNITS           "0.1Meters"
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "OTDR detection line length."
        ::= { zxAnOtdrDetectionLineConfEntry 2 }

    zxAnOtdrDetectLineAlias    OBJECT-TYPE
        SYNTAX          DisplayString (SIZE (0..32))
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION     "OTDR detection line alias."
        ::= { zxAnOtdrDetectionLineConfEntry 3 }

    zxAnOtdrDetectLinePositionDiff    OBJECT-TYPE
        SYNTAX          Integer32(0..10000)
        UNITS           "Meters"
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION     "OTDR detection line position difference threshold."
        DEFVAL { 3 }
        ::= { zxAnOtdrDetectionLineConfEntry 4 }

    zxAnOtdrDetectLineAbnPeakDiff   OBJECT-TYPE
        SYNTAX          Integer32(0..10000)
        UNITS           "0.01dB"
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION     "OTDR detection line abnormal reflective peak
                         difference threshold."
        DEFVAL { 50 }
        ::= { zxAnOtdrDetectionLineConfEntry 5 }

    zxAnOtdrDetectLineStatus    OBJECT-TYPE
        SYNTAX          INTEGER
            {
                normal(1),
                peakAbnormal(2),
                fiberBroken(3),
                unknown(255)
            }
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "OTDR detection line status.
             A zxAnOtdrDetectionLineAbnAlm trap will be sent when
             the zxAnOtdrDetectionLineStatus is peakAbnormal or fiberBroken.
             A zxAnOtdrDetectionLineAbnClr trap will be sent when
             the zxAnOtdrDetectionLineStatus is normal."
        ::= { zxAnOtdrDetectionLineConfEntry 6 }

