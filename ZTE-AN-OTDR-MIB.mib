-- 1. OTDR Global Objects -----------------------------------------------------
-------------------------------------------------------------------------------
-- 1.1 OTDR Capacity Global Objects
-------------------------------------------------------------------------------

    zxAnOtdrCapacityGlobalObjects    OBJECT IDENTIFIER
        ::= { zxAnOtdrGlobalObjects 1 }

    zxAnOtdrWaveLengthList    OBJECT-TYPE
        SYNTAX          DisplayString (SIZE (0..255))
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "The wave length supported by OTDR. The value divided by '|'.
             The unit of wave length is nm.
             PON OLTs support 1625.
             eOMUs support 1550|1625."
        ::= { zxAnOtdrCapacityGlobalObjects 1 }

    zxAnOtdrTestPulseWidthList    OBJECT-TYPE
        SYNTAX          DisplayString (SIZE (0..255))
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "The pulse width supported by OTDR. The value divided by '|'.
             The unit of pulse width is ns.
             The value 200001 means 2.5ns.
             e.g., 5|10|30|100|200|500|10000|20000|200001."
        ::= { zxAnOtdrCapacityGlobalObjects 2 }

    zxAnOtdrTotalPorts    OBJECT-TYPE
        SYNTAX          Integer32(1..256)
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The total port number of OTDR."
        ::= { zxAnOtdrCapacityGlobalObjects 3 }

    zxAnOtdrTestSeqCodeLenList    OBJECT-TYPE
        SYNTAX          DisplayString (SIZE (0..255))
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "The sequence code length supported by OTDR. The value divided
             by '|'.
             PON OLTs support 512|1024|2048|4096.
             eOMUs support 64|256|512|1024."
        ::= { zxAnOtdrCapacityGlobalObjects 4 }

    zxAnOtdrTestSeqCodeWidthList    OBJECT-TYPE
        SYNTAX          DisplayString (SIZE (0..255))
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "The sequence code width supported by OTDR. The value divided
             by '|'.
             The unit of pulse width is ns.
             PON OLTs support 5|10|20|30|50.
             eOMUs support 5|10|20|30."
        ::= { zxAnOtdrCapacityGlobalObjects 5 }

    zxAnOtdrDualWavelengthTestMode    OBJECT-TYPE
        SYNTAX          INTEGER{
            cascade(1),
            nonCascade(2)
        }
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "The test mode of dual wavelength.
             cascade(1): dual wavelength testing on two NEs by cascade.
             nonCascade(2): dual wavelength testing on one NE."
        ::= { zxAnOtdrCapacityGlobalObjects 6 }

    zxAnOtdrCapabilities OBJECT-TYPE
        SYNTAX      BITS {
            fastHealthPrfDiff(0),
            seqCodeLen(1),
            routineTaskTestMode(2),
            priorityTaskTest(3),
            reflectorTest(4),
            globalTestParamPrf(5),
            opticalFiberLabefaction(6),
            bothwayTest(7),
            controlCardIndependentTest(8)
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The OTDR capabilities of this equipment.

             The bit value 1 means the function represented by this bit is
             supported by this equipment.
             - fastHealthPrfDiff(0):
               value 1 means supporting zxAnOtdrIfHealthTestParamPrf and
               zxAnOtdrTestType.
             - seqCodeLen(1):
               value 1 means supporting zxAnOtdrTestParamPrfSeqCodeLen.
             - routineTaskTestMode(2):
               value 1 means supporting zxAnOtdrRoutineTaskTestMode.
             - priorityTaskTest(3):
               value 1 means supporting zxAnOtdrFastTestPriority and
               zxAnOtdrFastTestRequestSource.
             - reflectorTest(4):
               value 1 means supporting zxAnOtdrIfReflectorTestParamPrf and
               zxAnOtdrTestType.
             - globalTestParamPrf(5):
               value 1 means supporting global test parameter profile.
             - opticalFiberLabefaction(6):
               value 1 means supporting optical fiber labefaction predication.
             - bothwayTest(7):
               value 1 means supporting zxAnOtdrIfBothwayTestParamPrf and 
               zxAnOtdrTestType.
             - controlCardIndependentTest(8):
               value 1 means supporting the control card test without line card."
        ::= { zxAnOtdrCapacityGlobalObjects 51 }

-------------------------------------------------------------------------------
--  1.2 OTDR Environment Global Objects
-------------------------------------------------------------------------------
-- 1.2.1 OTDR Environment Control Card Global Objects
-------------------------------------------------------------------------------

    zxAnOtdrEnvGlobalObjects            OBJECT IDENTIFIER
                                        ::= { zxAnOtdrGlobalObjects 2 }

    zxAnOtdrEnvCtrlCardGlobalObjects    OBJECT IDENTIFIER
                                        ::= { zxAnOtdrEnvGlobalObjects 1 }

    zxAnOtdrCtrlCardCurrentTemp OBJECT-TYPE
       SYNTAX       Integer32
       UNITS        "Centigrade"
       MAX-ACCESS   read-only
       STATUS       current
       DESCRIPTION
            "The current control card temperature of OTDR equipment. "
       ::= { zxAnOtdrEnvCtrlCardGlobalObjects 1 }

    zxAnOtdrCtrlCardTempHighThresh OBJECT-TYPE
       SYNTAX       Integer32(75..95)
       UNITS        "Centigrade"
       MAX-ACCESS   read-write
       STATUS       current
       DESCRIPTION
            "The high threshold of the control card temperature, the
             zxAnOtdrEnvCtrlCardHighTempAlm trap should be generated when
             zxAnOtdrCtrlCardCurrentTemp is higher than it.
             Otherwise, zxAnOtdrEnvCtrlCardHighTempClr trap should be generated
             when zxAnOtdrCtrlCardCurrentTemp is lower than it."
       DEFVAL { 95 }
       ::= { zxAnOtdrEnvCtrlCardGlobalObjects 2 }

    zxAnOtdrCtrlCardTempLowThresh OBJECT-TYPE
       SYNTAX       Integer32(-45..-25)
       UNITS        "Centigrade"
       MAX-ACCESS   read-write
       STATUS       current
       DESCRIPTION
            "The low threshold of the control card temperature, the
             zxAnOtdrEnvCtrlCardLowTempAlm trap should be generated when
             zxAnOtdrCtrlCardCurrentTemp is lower than it.
             Otherwise, zxAnOtdrEnvCtrlCardLowTempClr trap should be generated
             when zxAnOtdrCtrlCardCurrentTemp is higher than it."
       DEFVAL { -45 }
       ::= { zxAnOtdrEnvCtrlCardGlobalObjects 3 }

