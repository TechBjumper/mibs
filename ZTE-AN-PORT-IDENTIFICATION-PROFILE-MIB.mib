    zxAnPortIdOperDhcpV4L2RAEnable OBJECT-TYPE
        SYNTAX      INTEGER
        {
            enabled(1),    
            disabled(2)                                      
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION  
           "The operator profile switch used to activate or deactivate interface
            DHCPv4 layer-2 relay agent.
            
            There are two states:
            - enabled: the DHCPv4 layer-2 relay agent is activated.
            - disabled: the DHCPv4 layer-2 relay agent is deactivated."
        DEFVAL      { disabled }          
        ::= { zxAnPortIdOperatorPrfEntry 11 }
    
    zxAnPortIdOperDhcpV4L2RATrust OBJECT-TYPE 
        SYNTAX      TruthValue 
        MAX-ACCESS  read-create 
        STATUS      current 
        DESCRIPTION 
            "DHCPv4 layer-2 relay agent trust flag of the operator profile. 
            
             To set this variable, it must be in one SNMP set operation together
             with variable zxAnPortIdOperDhcpV4L2RAPolicy."
        DEFVAL      { false }
        ::= { zxAnPortIdOperatorPrfEntry 12 }
        
    zxAnPortIdOperDhcpV4L2RAPolicy OBJECT-TYPE 
        SYNTAX      INTEGER 
        {
            keep(1), 
            replace(2), 
            discard(3), 
            add(4)
        }
        MAX-ACCESS  read-create 
        STATUS      current 
        DESCRIPTION 
            "DHCPv4 layer-2 relay agent trust policy of the operator profile.
             
             To set this variable, it must be in one SNMP set operation together
             with variable zxAnPortIdOperDhcpV4L2RATrust.
             
             In addition, a manager must comply with the following rules:
             - when trust flag is true, policy can be set to keep or replace, 
             - when trust flag is false, policy can be set to discard or add."
        DEFVAL      { add }
        ::= { zxAnPortIdOperatorPrfEntry 13 } 
        
