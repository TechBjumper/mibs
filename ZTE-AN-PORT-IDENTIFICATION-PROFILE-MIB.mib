-- 3. zxAnPortIdCircuitIdSyntaxTable
--------------------------------------------------------------------------------
    zxAnPortIdCircuitIdSyntaxTable OBJECT-TYPE 
        SYNTAX      SEQUENCE OF ZxAnPortIdCircuitIdSyntaxEntry 
        MAX-ACCESS  not-accessible 
        STATUS      current 
        DESCRIPTION 
            "This table is to provide circuit ID syntax configuration. One entry
             in this table reflects a syntax item define by a manager, which is
             a component of a port identification format profile." 
        ::= { zxAnPortIdPrfObjects 3 }
    
    zxAnPortIdCircuitIdSyntaxEntry OBJECT-TYPE 
        SYNTAX      ZxAnPortIdCircuitIdSyntaxEntry
        MAX-ACCESS  not-accessible 
        STATUS      current 
        DESCRIPTION 
            "An entry in zxAnPortIdCircuitIdSyntaxTable, which is index by the
             zxAnPortIdFormatPrfName and zxAnPortIdCidComponentIndex.
        
            If zxAnPortIdCidComponentType is standardVar(1) or extendedVar(2),
            zxAnPortIdCidComponentString can not be configured.
            
            If zxAnPortIdCidComponentType is separator(3),
            zxAnPortIdCidComponentWidth and zxAnPortIdCidComponentString 
            can not be configured.
            
            If zxAnPortIdCidComponentType is userDefinedString(4),
            zxAnPortIdCidComponentId,zxAnPortIdCidComponentWidth 
            can not be configured.
             
            After an entry is created, all variables can not be modified."
        INDEX       { zxAnPortIdFormatPrfName, zxAnPortIdCidComponentIndex } 
        ::= { zxAnPortIdCircuitIdSyntaxTable 1 }
    
    ZxAnPortIdCircuitIdSyntaxEntry ::= SEQUENCE {
        zxAnPortIdCidComponentIndex         Integer32,
        zxAnPortIdCidComponentType          INTEGER, 
        zxAnPortIdCidComponentId            Integer32,
        zxAnPortIdCidComponentWidth         Integer32,
        zxAnPortIdCidComponentString        DisplayString,
        zxAnPortIdCidComponentProfile       DisplayString,
        zxAnPortIdCidComponentRowStatus     RowStatus
    }
        
    zxAnPortIdCidComponentIndex OBJECT-TYPE
        SYNTAX      Integer32(1..30)
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "A key that identifies a component in the circuit ID syntax of
             a port identification format profile."
        REFERENCE   "TR-141 Section 6.16"
        ::= { zxAnPortIdCircuitIdSyntaxEntry 1 }        
        
    zxAnPortIdCidComponentType OBJECT-TYPE 
        SYNTAX      INTEGER 
        {   
            standardVar(1),
            extendedVar(2),
            separator(3),
            userDefinedString(4)
        }
        MAX-ACCESS  read-create 
        STATUS      current 
        DESCRIPTION 
            "An attribute that identifies the type of this component.
             The following types are possible:
             standardVar - A TR-101 based variable(1: access node id,
                           2: chassis, 3: rack, 4: frame, 5: slot,
                           6: sub-slot, 7: port, 8: vpi, 9: vci, 10: q-vid,
                           11: ethernet-priority)
             extendedVar - A proprietary variable(1: onu id, 2: gemPort, 
                           3: cvlan, 4: svlan, 5: sn, 6: loid, 7: loidpw,
                           8: onu-number, 9: onu-info, 10: access-mode,
                           11: access-node-type, 12: xpi, 13: xci, 14: vport,
                           15: user-defined-cid, 16:src-mac, 17: onu-name, 
                           18: user-defined-rid)
             separator   - A delimiter (6: space)
             userDefinedString - Information configured by the network operator.
             "
        REFERENCE   "TR-141 Section 6.16"
        DEFVAL { standardVar }
        ::= { zxAnPortIdCircuitIdSyntaxEntry 2 }
        
    zxAnPortIdCidComponentId OBJECT-TYPE 
        SYNTAX      Integer32
        MAX-ACCESS  read-create
        STATUS      current 
        DESCRIPTION 
            "A unique and content sensitive identifier for the specific 
             definition of this component. The following identifiers are
             expected:
             - A row number in table 2 of TR-101 (R-126).
             - An index into a proprietary managed entity that specifies
               possible proprietary variables.
             - An index into a proprietary managed entity that specifies 
               possible constant character strings."
        REFERENCE   "TR-141 Section 6.16"
        ::= { zxAnPortIdCircuitIdSyntaxEntry 3 }
    
    zxAnPortIdCidComponentWidth OBJECT-TYPE 
        SYNTAX      Integer32(0..5)
        MAX-ACCESS  read-create 
        STATUS      current
        DESCRIPTION
            "The width of the port identification circuit ID syntax component. 
            If the length of component is less than the width then fill zero in 
            front of the component.
            Value 0 means unconfigured."
        DEFVAL { 0 }
        ::= { zxAnPortIdCircuitIdSyntaxEntry 4 }

    zxAnPortIdCidComponentString OBJECT-TYPE 
        SYNTAX      DisplayString (SIZE (0..50))
        MAX-ACCESS  read-create 
        STATUS      current 
        DESCRIPTION 
            "Information configured by the network operator."
        DEFVAL { "" }
        ::= { zxAnPortIdCircuitIdSyntaxEntry 5 }
        
    zxAnPortIdCidComponentProfile OBJECT-TYPE 
        SYNTAX      DisplayString(SIZE(0..32))
        MAX-ACCESS  read-create 
        STATUS      current 
        DESCRIPTION 
            "At present, this object is the syntax profile of the ONU 
            information. It is supported when the component is onu-info 
            (when zxAnPortIdCidComponentType is extendedVar(2) and 
            zxAnPortIdCidComponentType is onu-info(9)). 
            The profile is also defined in zxAnPortIdFormatPrfTable. "
        DEFVAL { "" }
        ::= { zxAnPortIdCircuitIdSyntaxEntry 6 }

    zxAnPortIdCidComponentRowStatus OBJECT-TYPE 
        SYNTAX      RowStatus
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION 
            "The status of this conceptual row.
                        
             The supported actions of this conceptual row only include:
             'createAndGo' and 'destroy'. 
             
             To create a row in this table, a manager must set this object to 
             createAndGo(4), and must comply with the rule described in 
             zxAnPortIdCircuitIdSyntaxEntry.    
             To delete a row in this table, a manager must set this object to 
             destroy(6).             
             To get this object, the agent always returns active(1)."
        ::= { zxAnPortIdCircuitIdSyntaxEntry 50 }
        
