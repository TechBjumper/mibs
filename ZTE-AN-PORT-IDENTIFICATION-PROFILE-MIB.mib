    zxAnPortIdFormatPrfTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnPortIdFormatPrfEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION 
            "This table contains information of the port identification format
             profile configuration. One entry in this table reflects a format
             profile defined by a manager."
        ::= { zxAnPortIdPrfObjects 2 }
        
    zxAnPortIdFormatPrfEntry OBJECT-TYPE
        SYNTAX      ZxAnPortIdFormatPrfEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION 
            "An entry in zxAnPortIdFormatPrfTable, which is indexed by the
             zxAnPortIdFormatPrfName.
             
             To create a format profile, variable zxAnPortIdFormatPrfRowStatus
             must be included in one SNMP set operation."
        INDEX       {zxAnPortIdFormatPrfName}
        ::= { zxAnPortIdFormatPrfTable 1 }
        
    ZxAnPortIdFormatPrfEntry ::= SEQUENCE {
        zxAnPortIdFormatPrfName         DisplayString,
        zxAnPortIdFormatPrfRowStatus    RowStatus
    }
    
    zxAnPortIdFormatPrfName OBJECT-TYPE
        SYNTAX      DisplayString(SIZE(1..32))
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION  
           "A port identification format profile name."
        ::= {zxAnPortIdFormatPrfEntry 1}
        
    zxAnPortIdFormatPrfRowStatus OBJECT-TYPE
        SYNTAX      RowStatus
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The status of this conceptual row.
                        
             The supported actions of this conceptual row only include:
             'createAndGo' and 'destroy'. 
             
             To create a row in this table, a manager must set this object to 
             createAndGo(4), and must comply with the rule described in 
             zxAnPortIdFormatPrfEntry.    
             To delete a row in this table, a manager must set this object to 
             destroy(6).             
             To get this object, the agent always returns active(1)."
        ::= { zxAnPortIdFormatPrfEntry 50 }
        
