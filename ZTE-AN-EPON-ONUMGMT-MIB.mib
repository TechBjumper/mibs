        ::= { zxAnEponOnuPortVlanTable 1 } 
        
    ZxAnEponOnuPortVlanEntry ::=
        SEQUENCE  {                
                  zxAnEponOnuPortVlanMode            INTEGER,
                  zxAnEponOnuPortVlanPVid            Integer32, 
                  zxAnEponOnuPortVlanTpid            Integer32, 
