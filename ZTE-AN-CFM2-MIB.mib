        zxAnCfm2MepTransLbmDestMepId       Dot1agCfmMepIdOrZero,
        zxAnCfm2MepTransLbmDestMepType     INTEGER,
        zxAnCfm2MepTransLbmDataTlvLength   Integer32,
        zxAnCfm2MepTransLbmVlanPriority    Integer32,
        zxAnCfm2MepTransLbmTimeout         Integer32,
        zxAnCfm2MepTransLbmResultOk        INTEGER,
        zxAnCfm2MepTransLbmSeqNumber       Unsigned32,
        zxAnCfm2MepNextLbmTransId          Unsigned32,
        zxAnCfm2MepLbrInInOrder            Counter32,
        zxAnCfm2MepLbrInOutOfOrder         Counter32,
        zxAnCfm2MepLbrBadMsdu              Counter32,
        zxAnCfm2MepLbrOut                  Counter32,
        zxAnCfm2MepLbMaxLatency            Integer32,
        zxAnCfm2MepLbMinLatency            Integer32,
        zxAnCfm2MepLbAvgLatency            Integer32
    }

