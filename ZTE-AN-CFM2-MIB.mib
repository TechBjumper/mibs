-- 2.3.6 CFM2 MEP Performance Measurement Objects
--------------------------------------------------------------------------------

--------------------------------------------------------------------------------
-- 2.3.6.2 CFM2 MEP DM Table
--------------------------------------------------------------------------------

    zxAnCfm2MepDmOperTable   OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnCfm2MepDmOperEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Maintenance Association End Point (MEP) performance
             measurement operation table stores all the managed objects
             for sending DMM.

             Steps to use entries in this table:

             1) Wait for zxAnCfm2MepDmEnable value to be
                disabled. To do this do this sequence:
                a. an SNMP GET for both snmpSetSerialNo and
                   zxAnCfm2MepDmEnable objects (in same SNMP PDU).
                b. Check if value for zxAnCfm2MepDmEnable is disabled.
                   - if not, wait x seconds, go to step a above.
                   - if enabled, save the value of snmpSetSerialNo and go
                   to step 2) below
             2) Change zxAnCfm2MepDmEnable value from disabled to
                enabled to ensure no other management entity will use
                the service. In order to not disturb a possible other NMS
                do this by sending an SNMP SET for both snmpSetSerialNo
                and zxAnCfm2MepDmEnable and other delay measurement
                objects (such as zxAnCfm2MepDmPriority,...) (in same SNMP
                PDU, and make sure snmpSetSerialNo is the first varbind).
                For the snmpSetSerialNo varbind, use the value that you
                obtained in step 1) a. This ensures that two cooperating
                NMSes will not step on each others toes.
             3) Check the value of zxAnCfm2MepDmResultOk to
                find out if the operation was successfully initiated or not.
             4) Monitor the value of zxAnCfm2MepDmEnable.
                When it is reset to false, the last DMM has been transmitted.
             5) Get the results of the test from zxAnCfm2MepDmRltTable."
        ::= { zxAnCfm2PerfMeasureObjects 2 }

    zxAnCfm2MepDmOperEntry  OBJECT-TYPE
        SYNTAX      ZxAnCfm2MepDmOperEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The Delay Measurement table entry."
        INDEX   { zxAnCfm2MdIndex, zxAnCfm2MaIndex, zxAnCfm2MpIdentifier,
                  zxAnCfmMepDmTestId }
        ::= { zxAnCfm2MepDmOperTable 1 }

    ZxAnCfm2MepDmOperEntry ::= SEQUENCE {
        zxAnCfmMepDmTestId                Integer32,
        zxAnCfm2MepDmEnable               INTEGER,
        zxAnCfm2MepDmTargetMepId          Dot1agCfmMepIdOrZero,
        zxAnCfm2MepDmFrameSize            INTEGER,
        zxAnCfm2MepDmAppType              INTEGER,
        zxAnCfm2MepDmInterval             INTEGER,
        zxAnCfm2MepDmDuration             Integer32,
        zxAnCfm2MepDmPriority             Integer32,
        zxAnCfm2MepDmOperType             INTEGER,
        zxAnCfm2MepDmResultOk             INTEGER
        }

    zxAnCfmMepDmTestId                  OBJECT-TYPE
        SYNTAX      Integer32(1..8)
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The frame delay measurement identifier of the specified MEP.
             8 measurements can be simultaneously activated on the MEP."
        ::= { zxAnCfm2MepDmOperEntry 1 }

    zxAnCfm2MepDmEnable                 OBJECT-TYPE
        SYNTAX      INTEGER {
            enabled(1),
            disabled(2)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "Enable/disable the DM function.
             If the value is disable, the DM test can not be started."
        DEFVAL { disabled }
        ::= { zxAnCfm2MepDmOperEntry 2 }

    zxAnCfm2MepDmTargetMepId            OBJECT-TYPE
        SYNTAX      Dot1agCfmMepIdOrZero
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The Maintenance association End Point Identifier of another MEP in
             the same Maintenance Association to which the DM is to be sent.
             It will be used if DMM destination MAC address type is unicast."
        DEFVAL { 0 }
        ::= { zxAnCfm2MepDmOperEntry 3 }

    zxAnCfm2MepDmFrameSize              OBJECT-TYPE
        SYNTAX      INTEGER {
            size66(1),
            size128(2),
            size256(3),
            size512(4),
            size1024(5),
            size1280(6),
            size1518(7)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The DM frame size."
        DEFVAL { size66 }
        ::= { zxAnCfm2MepDmOperEntry 4 }

    zxAnCfm2MepDmAppType                OBJECT-TYPE
        SYNTAX      INTEGER {
            singleEnded(1),
            dualEnded(2)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The application type of the DM."
        DEFVAL { singleEnded }
        ::= { zxAnCfm2MepDmOperEntry 5 }

    zxAnCfm2MepDmInterval               OBJECT-TYPE
        SYNTAX      INTEGER {
            interval1s(1),
            interval10s(2),
            interval60s(3),
            interval100s(4)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The interval at which DMMs are sent by an MEP."
        DEFVAL { interval1s }
        ::= { zxAnCfm2MepDmOperEntry 6 }

    zxAnCfm2MepDmDuration               OBJECT-TYPE
        SYNTAX      Integer32(10..86400)
        UNITS       "Seconds"
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The duration of DM test.
             It can be configured when zxAnCfm2MepDmOperType is onDemand."
        DEFVAL { 60 }
        ::= { zxAnCfm2MepDmOperEntry 7 }

    zxAnCfm2MepDmPriority               OBJECT-TYPE
        SYNTAX      Integer32(0..7)
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The priority. 3 bit value to be used in the VLAN tag,
             if present in the transmitted frame."
        DEFVAL { 7 }
        ::= { zxAnCfm2MepDmOperEntry 8 }

    zxAnCfm2MepDmOperType   OBJECT-TYPE
        SYNTAX      INTEGER {
            onDemand(1),
            proactive(2)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The operation type of the DM."
        DEFVAL { onDemand }
        ::= { zxAnCfm2MepDmOperEntry 9 }

    zxAnCfm2MepDmResultOk               OBJECT-TYPE
        SYNTAX      INTEGER {
            yes(1),
            no(2)
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The result of the operation:
             yes(1): The Delay Measurement Message(s) will be (or has been)
                     sent.
             no(2): The Delay Measurement Message(s) will not be sent."
        DEFVAL { yes }
        ::= { zxAnCfm2MepDmOperEntry 30 }

--------------------------------------------------------------------------------
-- 2.3.6.3 CFM2 MEP DM result Table
--------------------------------------------------------------------------------
     zxAnCfm2MepDmRltTable              OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnCfm2MepDmRltEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table includes multicast or unicast frame delay measurement
             result."
        ::= { zxAnCfm2PerfMeasureObjects 3 }

    zxAnCfm2MepDmRltEntry               OBJECT-TYPE
        SYNTAX      ZxAnCfm2MepDmRltEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The delay measurement result table entry."
        INDEX   { zxAnCfm2MdIndex, zxAnCfm2MaIndex, zxAnCfm2MpIdentifier,
                  zxAnCfmMepDmTestId, zxAnCfm2DmRemoteMepId }
        ::= { zxAnCfm2MepDmRltTable 1 }

    ZxAnCfm2MepDmRltEntry ::= SEQUENCE {
        zxAnCfm2DmRemoteMepId             Dot1agCfmMepId,
        zxAnCfm2MepDmStartTime            DateAndTime,
        zxAnCfm2MepDmEndTime              DateAndTime,
