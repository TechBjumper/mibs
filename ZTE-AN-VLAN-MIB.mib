-- 2.3.5  zxAnVlanIfExtTransTable
-------------------------------------------------------------------------------
    zxAnVlanIfExtTransTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnVlanIfExtTransEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "VLAN translation extension table. 
             This table is an extension of the table zxAnVlanIfTransTable.
             This table includes VLAN translation information for single tagged 
             packets, and double tagged packets."
        ::= { zxAnVlanTransObjects 5 }
    
    zxAnVlanIfExtTransEntry OBJECT-TYPE
        SYNTAX      ZxAnVlanIfExtTransEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "VLAN Translation extension entry."
        INDEX   {
            ifIndex, 
            zxAnSubIfIndex,
            zxAnVlanIfExtTransIngressVid,
            zxAnVlanIfExtTransIngressCVid
        }
        ::= { zxAnVlanIfExtTransTable 1 }
    
    ZxAnVlanIfExtTransEntry ::= SEQUENCE {
        zxAnVlanIfExtTransIngressVid   VlanId,
        zxAnVlanIfExtTransIngressCVid  Integer32,
        zxAnVlanIfExtTransVid          VlanId,
        zxAnVlanIfExtTransCVid         Integer32,
        zxAnVlanIfExtTransCos          Integer32,
        zxAnVlanIfExtTransCtagCos      Integer32,
        zxAnVlanIfExtTransRowStatus    RowStatus
        }

    zxAnVlanIfExtTransIngressVid OBJECT-TYPE
        SYNTAX      VlanId 
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "Ingress VID for VLAN translation."
        ::= { zxAnVlanIfExtTransEntry 1 }
    
    zxAnVlanIfExtTransIngressCVid OBJECT-TYPE
        SYNTAX      Integer32 (0..4094) 
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "Ingress C-VID for VLAN translation.
             '0' means the packet to be translated is single tagged packet."
        ::= { zxAnVlanIfExtTransEntry 2 }
            
    zxAnVlanIfExtTransVid OBJECT-TYPE
        SYNTAX      VlanId 
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "VID after VLAN translation."
        ::= { zxAnVlanIfExtTransEntry 3 }
        
    zxAnVlanIfExtTransCVid OBJECT-TYPE
        SYNTAX      Integer32 (0..4094) 
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "C-VID after VLAN translation.
             '0' means the packet after VLAN translation is single tagged 
             packet."
        ::= { zxAnVlanIfExtTransEntry 4 }

    zxAnVlanIfExtTransCos OBJECT-TYPE
        SYNTAX      Integer32 (0..7|255) 
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "VLAN CoS after VLAN translation.
             Value '0xFF' indicates not configured."
        DEFVAL  { 255 }
        ::= { zxAnVlanIfExtTransEntry 5 }

    zxAnVlanIfExtTransCtagCos OBJECT-TYPE
        SYNTAX      Integer32 (0..7|255) 
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "CVLAN CoS after VLAN translation.
