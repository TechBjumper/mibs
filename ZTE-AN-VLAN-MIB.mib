    zxAnVlanExtPortListIIRack OBJECT-TYPE
        SYNTAX      Integer32 
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "Rack index for port list on the VLAN."
        ::= { zxAnVlanExtPortListIIEntry 1 }
        
    zxAnVlanExtPortListIIShelf OBJECT-TYPE
        SYNTAX      Integer32 
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "Shelf index for port list on the VLAN."
        ::= { zxAnVlanExtPortListIIEntry 2 }
        
    zxAnVlanExtPortListIISlot OBJECT-TYPE
        SYNTAX      Integer32 
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "Slot index for port list on the VLAN."
        ::= { zxAnVlanExtPortListIIEntry 3 }
        
    zxAnVlanExtPortListIIPort OBJECT-TYPE
        SYNTAX      Integer32 
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "Port index for port list on the VLAN.
             Value 0 means non-PON port, other values means EPON/GPON port."
        ::= { zxAnVlanExtPortListIIEntry 4 }
    
    zxAnVlanExtPortListIISegment OBJECT-TYPE
        SYNTAX      Integer32(1..16)
        MAX-ACCESS  not-accessible
        STATUS      current
