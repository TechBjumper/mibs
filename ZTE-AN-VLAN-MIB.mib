        STATUS      current
        DESCRIPTION
            "Untagged ports of the VLAN."
        ::= { zxAnVlanExtPortListIIEntry 6 }
    
    zxAnVlanExtPortTagPortListII OBJECT-TYPE
        SYNTAX      ZxAnPortList
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
