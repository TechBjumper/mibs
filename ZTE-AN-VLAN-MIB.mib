-- 2.1.9  zxAnVlanIfConfVlanTable
-------------------------------------------------------------------------------
    zxAnVlanIfConfVlanTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnVlanIfConfVlanEntry 
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "VLAN configuration table.
             This table is only used for getting VLAN information (network-side 
             SVLAN of service port function) when operating on EPON/GPON ports.
             This table is used for setting and getting VLAN information when 
             operating on uplink ports.
             This table is used for setting and getting VLAN information when 
             operating on DSL ports."
        ::= { zxAnVlanBaseObjects 9 }

    zxAnVlanIfConfVlanEntry  OBJECT-TYPE
        SYNTAX      ZxAnVlanIfConfVlanEntry 
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "VLAN configuration entry."
        INDEX  { ifIndex, zxAnSubIfIndex }
        ::= { zxAnVlanIfConfVlanTable 1 }

    ZxAnVlanIfConfVlanEntry  ::= SEQUENCE {
        zxAnVlanIfConfVlanTagVlanList       OCTET STRING,
        zxAnVlanIfConfVlanUntagVlanList     OCTET STRING
        }
       
    zxAnVlanIfConfVlanTagVlanList OBJECT-TYPE
        SYNTAX      OCTET STRING (SIZE(512))
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Tagged VLAN list in the port.
             A bit value of 1 indicates the associated VLAN is in the port."
        ::= { zxAnVlanIfConfVlanEntry 1 }
        
    zxAnVlanIfConfVlanUntagVlanList OBJECT-TYPE
        SYNTAX      OCTET STRING (SIZE(512))
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Untagged VLAN list in the port.
             A bit value of 1 indicates the associated VLAN is in the port."
        ::= { zxAnVlanIfConfVlanEntry 2 }
        
