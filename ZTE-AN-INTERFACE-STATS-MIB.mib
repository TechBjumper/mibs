    zxAnIfBandwidthUsingTable   OBJECT-TYPE
        SYNTAX      SEQUENCE  OF  ZxAnIfBandwidthUsingEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "Bandwidth using ratio and threshold table."
        ::=  { zxAnIfPerfObjects  30 }

    zxAnIfBandwidthUsingEntry   OBJECT-TYPE
        SYNTAX      ZxAnIfBandwidthUsingEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "Bandwidth using entry."
         INDEX   { ifIndex, zxAnSubIfIndex}
        ::=  { zxAnIfBandwidthUsingTable 1 }

    ZxAnIfBandwidthUsingEntry  ::=  SEQUENCE {
        zxAnIfInBandwidthUsingRatio              Integer32,
        zxAnIfOutBandwidthUsingRatio             Integer32,
        zxAnIfInBandwidthUsingThresh             Integer32,
        zxAnIfOutBandwidthUsingThresh            Integer32,
        zxAnIfInBandwidthUsingRatioHa            Integer32,          
        zxAnIfOutBandwidthUsingRatioHa           Integer32
       }

    zxAnIfInBandwidthUsingRatio OBJECT-TYPE
        SYNTAX          Integer32(0..100)
        UNITS           "percents"
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "An estimate of the interface's current received bandwidth using
            ratio."
        ::=  {  zxAnIfBandwidthUsingEntry  1  }

    zxAnIfOutBandwidthUsingRatio OBJECT-TYPE
        SYNTAX          Integer32(0..100)
        UNITS           "percents"
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "An estimate of the interface's current transmitted bandwidth using
            ratio."
        ::=  {  zxAnIfBandwidthUsingEntry  2  }

    zxAnIfInBandwidthUsingThresh OBJECT-TYPE
        SYNTAX          Integer32(0..100)
        UNITS           "percents"
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
            "The threshold of zxAnIfInBandwidthUsingRatio.
            When zxAnIfInBandwidthUsingRatio exceeds this threshold,
            the zxAnIfInBwUsingThreshExceedAlm trap should be generated.
            If zxAnIfInBandwidthUsingRatio always exceeds the threshold,
            only one zxAnIfInBwUsingThreshExceedAlm trap should be sent."
        DEFVAL {100}
        ::=  {  zxAnIfBandwidthUsingEntry  3  }

    zxAnIfOutBandwidthUsingThresh OBJECT-TYPE
        SYNTAX          Integer32(0..100)
        UNITS           "percents"
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
            "The threshold of zxAnIfOutBandwidthUsingRatio.
            When zxAnIfOutBandwidthUsingRatio exceeds this threshold,
            the zxAnIfOutBwUsingThreshExceedAlm trap should be generated.
            If zxAnIfOutBandwidthUsingRatio always exceeds the threshold,
            only one zxAnIfOutBwUsingThreshExceedAlm trap should be sent."
        DEFVAL {100}
        ::=  {  zxAnIfBandwidthUsingEntry  4  }

   zxAnIfInBandwidthUsingRatioHa OBJECT-TYPE
        SYNTAX          Integer32(0..10000000)
        UNITS           "0.00001percents"
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "The high accuracy value of zxAnIfInBandwidthUsingRatio."
        ::=  {  zxAnIfBandwidthUsingEntry  5  }

    zxAnIfOutBandwidthUsingRatioHa OBJECT-TYPE
        SYNTAX          Integer32(0..10000000)
        UNITS           "0.00001percents"
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "The high accuracy value of zxAnIfOutBandwidthUsingRatio"
        ::=  {  zxAnIfBandwidthUsingEntry  6  }

