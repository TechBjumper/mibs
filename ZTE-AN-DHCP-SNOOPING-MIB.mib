--------------------------------------------------------------------------------
-- 1. DHCP Snooping Global Objects
--------------------------------------------------------------------------------

    zxAnDhcpSnoopingCapabilities  OBJECT-TYPE 
        SYNTAX      BITS 
        {
            supportIpSrcGuardIpBindingTable(0), 
            supportIpv4SrcGuardIpBindIpMask(1)
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION   
            "The management capabilities of DHCP snooping.
             The bit value 1 means the function represented by this bit is 
             supported by this equipment.           
             supportIpSrcGuardIpBindingTable(0): value 1 means supporting 
                                       zxAnIpv4SrcGuardIpBindingTable and 
                                       zxAnIpv6SrcGuardIpBindingTable,
                                       zxAnDsDhcpV4StaticIpBindingTable and
                                       zxAnDsDhcpV6StaticIpBindingTable were 
                                       deprecated.
                                       
             supportIpv4SrcGuardIpBindIpMask(1): value 1 means supporting
                                      IPv4 mask configuration: 
                                      zxAnIpv4SrcGuardIpBindIpMask.
             NOTE: No recommended to display it in NMS."
        ::= { zxAnDhcpSnoopingGlobalObjects 1 }

--------------------------------------------------------------------------------
-- 2. IPv4 DHCP Snooping Objects
--------------------------------------------------------------------------------

--------------------------------------------------------------------------------
-- 2.1 IPv4 DHCP Snooping Global Objects
--------------------------------------------------------------------------------
    
    zxAnDsDhcpV4GlobalObjects    OBJECT IDENTIFIER ::= { zxAnDsDhcpV4Objects 1 }

    zxAnDsDhcpV4GlobalEnable OBJECT-TYPE
       SYNTAX      INTEGER
           {
               enabled(1),   
               disabled(2)
           }
       MAX-ACCESS  read-write
       STATUS      current
       DESCRIPTION
           "Enable or disable DHCPv4 snooping."
       DEFVAL { disabled }               
       ::= { zxAnDsDhcpV4GlobalObjects 1 }    
    
    zxAnDsDhcpV4ClearIpBindingEnable OBJECT-TYPE
       SYNTAX      INTEGER
           {
               enabled(1),   
               disabled(2)
           }
       MAX-ACCESS  read-write
       STATUS      deprecated
       DESCRIPTION
           "If this object is set to 'enabled', when interface become
            offline this interface's records in zxAnDsDhcpV4IpBindingTable 
            will be deleted. 
            Otherwise, this interface's records will not be deleted."
       DEFVAL { disabled }               
       ::= { zxAnDsDhcpV4GlobalObjects 2 } 
   
    zxAnDsDhcpV4OnlineUsers OBJECT-TYPE
       SYNTAX      Integer32 
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
           "The total number of DHCPv4 snooping online users."           
       ::= { zxAnDsDhcpV4GlobalObjects 3 }
    
--------------------------------------------------------------------------------
-- 2.2 IPv4 DHCP Snooping VLAN Table
--------------------------------------------------------------------------------

    zxAnDsDhcpV4VlanTable OBJECT-TYPE 
        SYNTAX      SEQUENCE OF ZxAnDsDhcpV4VlanEntry 
        MAX-ACCESS  not-accessible 
        STATUS      current 
        DESCRIPTION 
               "This table provides the mechanism to control 
                ipv4 dhcp snooping per vlan. 
                
                When a vlan is created in a device supporting this
                table, a corresponding entry of this table will be added.
                
                When a vlan is destroyed in a device supporting this
                table, a corresponding entry of this table will be deleted." 
        ::= { zxAnDsDhcpV4Objects 2 }
    
    zxAnDsDhcpV4VlanEntry OBJECT-TYPE 
        SYNTAX      ZxAnDsDhcpV4VlanEntry
        MAX-ACCESS  not-accessible 
        STATUS      current 
        DESCRIPTION 
               "A row instance contains the configuration to create
                 or destroy ipv4 dhcp snooping at each existing vlan."
        INDEX { zxAnDsDhcpV4Vid } 
        ::= { zxAnDsDhcpV4VlanTable 1 }
    
    ZxAnDsDhcpV4VlanEntry ::= SEQUENCE {
        zxAnDsDhcpV4Vid            VlanId,
        zxAnDsDhcpV4VlanRowStatus  RowStatus
        }
 
    zxAnDsDhcpV4Vid    OBJECT-TYPE
        SYNTAX      VlanId
        MAX-ACCESS  not-accessible 
        STATUS      current
        DESCRIPTION
               "This object indicates the vlan number on which 
                ipv4 dhcp snooping feature is configured." 
        ::= { zxAnDsDhcpV4VlanEntry 1 }
    
    zxAnDsDhcpV4VlanRowStatus OBJECT-TYPE
        SYNTAX      RowStatus
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
               "This object is used to manage the creation and deletion
                of rows in this table. It only supports 'active(1)'
                ,'createAndGo(4)'and 'destroy(6)' value.
                
                If this object is set to 'createAndGo(4)', the vlan 
                number on which dhcp snooping is active(1).
                
                Setting this object value to destroy(6),
                the vlan number on which dhcp snooping is deleted."
        ::= { zxAnDsDhcpV4VlanEntry 50 }    
             
                     
--------------------------------------------------------------------------------
-- 2.4 IPv4 DHCP Snooping Bindings Table
--------------------------------------------------------------------------------

    zxAnDsDhcpV4IpBindingTable OBJECT-TYPE 
        SYNTAX      SEQUENCE OF ZxAnDsDhcpV4IpBindingEntry 
        MAX-ACCESS  not-accessible 
        STATUS      current 
        DESCRIPTION 
               "This table provides the ipv4 dhcp snooping 
                bindings information learnt by the interface
                capable of this feature.
                
                The interfaces for which this feature must 
                be applicable are:
                IfType = Physical Interface(1)
                for PON card:
                SubIfType = ONU service port sub-interface(3)
                for DSL card:
                SubIfType = DSL PVC sub-interface(6)"        
        ::= { zxAnDsDhcpV4Objects 6 }
    
    zxAnDsDhcpV4IpBindingEntry OBJECT-TYPE 
        SYNTAX      ZxAnDsDhcpV4IpBindingEntry
        MAX-ACCESS  not-accessible 
        STATUS      current 
        DESCRIPTION 
               "A row instance contains the interface number,
                Mac address, service vlan number, IP address, 
                leased time, user vlan number, and status of 
                this instance."
        INDEX { ifIndex,
                zxAnSubIfIndex,
                zxAnDsDhcpV4IpBindingMac,
                zxAnDsDhcpV4IpBindingSVid } 
        ::= { zxAnDsDhcpV4IpBindingTable 1 }
    
    ZxAnDsDhcpV4IpBindingEntry ::= SEQUENCE {
        zxAnDsDhcpV4IpBindingMac            MacAddress,
        zxAnDsDhcpV4IpBindingSVid           VlanId,
        zxAnDsDhcpV4IpBindingIpAddress      IpAddress,
        zxAnDsDhcpV4IpBindingLeasedTime     DateAndTime,
        zxAnDsDhcpV4IpBindingUserVid        VlanId,
        zxAnDsDhcpV4IpBindingRowStatus      RowStatus
        }
        
    zxAnDsDhcpV4IpBindingMac  OBJECT-TYPE
        SYNTAX      MacAddress
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
               "This object indicates the MAC address of 
                an ipv4 dhcp client host."
        ::= { zxAnDsDhcpV4IpBindingEntry 1 }
        
    zxAnDsDhcpV4IpBindingSVid OBJECT-TYPE
        SYNTAX      VlanId
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
               "This object indicates the service vlan to
                which an ipv4 dhcp client host belongs. 
                Vlan range is from 1 to 4094."
        ::= { zxAnDsDhcpV4IpBindingEntry 2 }
    
    zxAnDsDhcpV4IpBindingIpAddress OBJECT-TYPE
        SYNTAX      IpAddress
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
               "This object indicates the allocated ipv4
                address of an ipv4 dhcp client host."
        ::= { zxAnDsDhcpV4IpBindingEntry 3 }

    zxAnDsDhcpV4IpBindingLeasedTime OBJECT-TYPE
        SYNTAX      DateAndTime
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
               "This object indicates the leased time of this ipv4 dhcp 
               bindings.The format is 2d-1d-1d,1d:1d:1d.1d.
                For example,  1992-5-26,13:30:15.0."
        ::= { zxAnDsDhcpV4IpBindingEntry 4 }
        
    zxAnDsDhcpV4IpBindingUserVid OBJECT-TYPE
        SYNTAX      VlanId
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
               "This object indicates the vlan to which 
                an ipv4 dhcp client host belongs. 
                Vlan range is from 1 to 4094."
        ::= { zxAnDsDhcpV4IpBindingEntry 5 }
        
    zxAnDsDhcpV4IpBindingRowStatus OBJECT-TYPE
        SYNTAX      RowStatus
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
               "This object is used to manage the deletion of rows
                in this table. This object only supports 'active(1)', 
                and 'destroy(6)' value. 

                Whenever an ipv4 dhcp binding is learnt by the interface, 
                an entry will be created in this table with its RowStatus
                object set to 'active(1)'. 
                
                Setting this object value to 'destroy(6)' will clear 
                the ipv4 dhcp binding information."
        ::= { zxAnDsDhcpV4IpBindingEntry 50}  
        
        
--------------------------------------------------------------------------------
-- 2.7 IPv4 DHCP Snooping Session Limit Table
--------------------------------------------------------------------------------

    zxAnDsDhcpV4IfSessionLimitTable OBJECT-TYPE 
        SYNTAX      SEQUENCE OF ZxAnDsDhcpV4IfSessionLimitEntry 
        MAX-ACCESS  not-accessible 
        STATUS      current 
        DESCRIPTION 
               "This object specifies the maximum number of DHCPv4 bindings
                allowed on this interface. This objects value will limit
                the number of entries in zxAnDsDhcpV4IpBindingTable. 
                
                The interfaces for which this feature must be applicable 
                are:
                IfType = Physical Interface(1)
                for PON card:
                SubIfType = ONU vport sub-interface(1)
                for DSL card:
                SubIfType = DSL PVC sub-interface(6)"
        ::= { zxAnDsDhcpV4Objects 9 }
    
    zxAnDsDhcpV4IfSessionLimitEntry OBJECT-TYPE 
        SYNTAX      ZxAnDsDhcpV4IfSessionLimitEntry
        MAX-ACCESS  not-accessible
        STATUS      current 
        DESCRIPTION 
               "A row instance contains the configuration of binding limit 
                at each interface capable of this feature."
        INDEX { ifIndex, 
                zxAnSubIfIndex }
        ::= { zxAnDsDhcpV4IfSessionLimitTable 1 }
    
    ZxAnDsDhcpV4IfSessionLimitEntry ::= SEQUENCE {
        zxAnDsDhcpV4IfSessionLimit    Integer32
         }
       
    zxAnDsDhcpV4IfSessionLimit OBJECT-TYPE
        SYNTAX      Integer32(0..255)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
               "This object specifies the maximum number of DHCPv4 bindings
                allowed on this interface. This objects value will limit
                the number of entries in zxAnDsDhcpV4IpBindingTable.when 
                the value is 0, that means no limit in the user interface."
        DEFVAL { 0 }
        ::= { zxAnDsDhcpV4IfSessionLimitEntry 1 }                                                                  
                                                                                                            
--------------------------------------------------------------------------------
-- 2.9 IPv4 DHCP Snooping Packets Statistics Table
--------------------------------------------------------------------------------
 
    zxAnDsDhcpV4IfStatsTable OBJECT-TYPE         
        SYNTAX      SEQUENCE OF ZxAnDsDhcpV4IfStatsEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION 
               "This table provides the ipv4 dhcp statistics 
                information received by by the interface
                capable of this feature.
                
                The interfaces for which this feature must 
                be applicable are:
                IfType = Physical Interface(1)
                for PON card:
                SubIfType = ONU vport sub-interface(2)
                for DSL card:
                SubIfType = DSL PVC sub-interface(6)"
        ::= { zxAnDsDhcpV4Objects 50 }

    zxAnDsDhcpV4IfStatsEntry OBJECT-TYPE
        SYNTAX      ZxAnDsDhcpV4IfStatsEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION 
               "A row instance contains the interface number,
                ipv4 dhcp packet information and reset ipv4
                dhcp packet statistics."
        INDEX { ifIndex, 
                zxAnSubIfIndex}
        ::= {zxAnDsDhcpV4IfStatsTable 1 }

    ZxAnDsDhcpV4IfStatsEntry ::= SEQUENCE {
        zxAnDsDhcpV4IfStatsDiscoverPkts   Counter32,
        zxAnDsDhcpV4IfStatsOfferPkts      Counter32,
        zxAnDsDhcpV4IfStatsRequestPkts    Counter32,
        zxAnDsDhcpV4IfStatsAckPkts        Counter32,
        zxAnDsDhcpV4IfStatsNackPkts       Counter32,
        zxAnDsDhcpV4IfStatsReleasePkts    Counter32,
        zxAnDsDhcpV4IfStatsDeclinePkts    Counter32,
        zxAnDsDhcpV4IfStatsInformPkts     Counter32,
        zxAnDsDhcpV4IfStatsReset          INTEGER
        }
    
    zxAnDsDhcpV4IfStatsDiscoverPkts    OBJECT-TYPE
        SYNTAX      Counter32        
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
               "This object indicates the discover number of 
                packets subjected to ipv4 dhcp snooping is received."
        ::= { zxAnDsDhcpV4IfStatsEntry 1 }      
        
    zxAnDsDhcpV4IfStatsOfferPkts  OBJECT-TYPE
        SYNTAX      Counter32        
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
               "This object indicates the offer number of 
                packets subjected to ipv4 dhcp snooping is received."
        ::= { zxAnDsDhcpV4IfStatsEntry 2 }         
       
    zxAnDsDhcpV4IfStatsRequestPkts    OBJECT-TYPE
        SYNTAX      Counter32        
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
               "This object indicates the request number of 
                packets subjected to ipv4 dhcp snooping is received."
        ::= { zxAnDsDhcpV4IfStatsEntry 3 }      
        
    zxAnDsDhcpV4IfStatsAckPkts  OBJECT-TYPE
        SYNTAX      Counter32        
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
               "This object indicates the ack number of 
                packets subjected to ipv4 dhcp snooping is received."
        ::= { zxAnDsDhcpV4IfStatsEntry 4 } 
        
    zxAnDsDhcpV4IfStatsNackPkts  OBJECT-TYPE
        SYNTAX      Counter32        
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
               "This object indicates the nack number of 
                packets subjected to ipv4 dhcp snooping is received."
        ::= { zxAnDsDhcpV4IfStatsEntry 5 }      
        
    zxAnDsDhcpV4IfStatsReleasePkts   OBJECT-TYPE
        SYNTAX      Counter32        
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
               "This object indicates the release number of 
                packets subjected to ipv4 dhcp snooping is received."
        ::= { zxAnDsDhcpV4IfStatsEntry 6 } 
        
    zxAnDsDhcpV4IfStatsDeclinePkts  OBJECT-TYPE
        SYNTAX      Counter32        
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
               "This object indicates the decline number of 
                packets subjected to ipv4 dhcp snooping is received."
        ::= { zxAnDsDhcpV4IfStatsEntry 7 }      
        
    zxAnDsDhcpV4IfStatsInformPkts  OBJECT-TYPE
        SYNTAX      Counter32        
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
               "This object indicates the inform number of 
                packets subjected to ipv4 dhcp snooping is received."
        ::= { zxAnDsDhcpV4IfStatsEntry 8 }
        
    zxAnDsDhcpV4IfStatsReset  OBJECT-TYPE
        SYNTAX      INTEGER 
            { 
                reset(1) 
            }        
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
               "This object indicates whether ipv4 dhcp statistics
                is cleared at this interface. 
                
                If this object is set to 'reset(1)', ipv4 dhcp
                statistics at this interface will be cleared."
        ::= { zxAnDsDhcpV4IfStatsEntry 100 }
        
     

