        ifIndex                                         FROM IF-MIB
        ZxAnSubIfIndex                                  FROM ZTE-AN-TC-MIB
        zxAnInterface                                   FROM ZTE-AN-SMI
        HCPerfCurrentCount, HCPerfTimeElapsed           FROM HC-PerfHist-TC-MIB;
    
