--------------------------------------------------------------------------------
-- 01. zxAnIeee1588Capabilities
--------------------------------------------------------------------------------
   zxAnIeee1588Capabilities     OBJECT-TYPE 
       SYNTAX   BITS{           
                from201602To201603(0),
                add1ppsSrcDelayCompensation(1)
                } 
       MAX-ACCESS       read-only
       STATUS           current
       DESCRIPTION      "The IEEE1588 capabilities of this equipment.
            The bit value 1 means the function represented by this bit is
            supported by this equipment.            
            bit 0: value 1 means the following changes,
                  adding zxAnPtpRemoteSrcPriority to the zxAnPtpRemoteSrcTable,
                  adding zxAn1588LocalUtcTime to the zxAnIeee1588TimeStatusInfoObjs,
                  adding zxAnPtpProtocolIpAddrPfxLen,
                         zxAnPtpProtocolGatewayIpAddrType and
                         zxAnPtpProtocolGatewayIpAddress to the 
                         zxAnPtpTable,
                  adding zxAnSyncTime1ppsSrcFormat,
                         zxAnSyncTime1ppsSrcCurrentQL,
                         zxAnSyncTime1ppsSrcTimeAccuracy and
                         zxAnSyncTime1ppsSrcTimeSrcType to the 
                         zxAnSyncTime1ppsSrcTable.                             
            bit 1: value 1 means the following changes,
                  adding zxAnSyncTime1ppsSrcDlyCompensate to the 
                  zxAnSyncTime1ppsSrcTable.
            NOTE: No recommended to display it in NMS."
       ::=  {  zxAnIeee1588GlobalObjects 1  }  
       
-------------------------------------------------------------------------------
-- 03. zxAnIeee1588AlmThreshObjects
-------------------------------------------------------------------------------
    zxAn1588FreqQLThresh OBJECT-TYPE
        SYNTAX      INTEGER {
            qlprc(2),
            qlssua(4),
            qlssub(8),
            qlsec(11)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "System frequency quality level threshold.
             A zxAnAccuracyDegradedAlm alarm will be generated when system 
             frequency quality level is below the following threshold.
             qlprc(2): quality level of primary reference clock, it is defined 
                       in recommendation G.811.
             qlssua(4): quality level of type I/V slave clock, it is defined 
                       in recommendation G.812.
             qlssub(8): quality level of type VI slave clock, it is defined 
                       in recommendation G.812.             
             qlsec(11): quality level of SDH equipment clock, it is defined 
                       in recommendation G.813."              
        DEFVAL { qlssua }
        ::= {zxAnIeee1588AlmThreshObjects 1}
    
    zxAnPtpClockTraceClassThresh OBJECT-TYPE
        SYNTAX      Integer32(0..255)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "PTP clock traceability class threshold.
             A zxAnPtpClockTCBelowThreshAlm alarm will be generated when PTP 
             traceability clock class is below threshold."
        DEFVAL { 135 }
        ::= {zxAnIeee1588AlmThreshObjects 2}
    
    zxAn1ppsInputSignalQLThresh OBJECT-TYPE
        SYNTAX      INTEGER {
            locked(1),
            qlprc(2),
            qlssua(4),
            qlssub(8),
            qlsec(11)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "1PPS input signal quality level threshold.
             A zxAn1ppsInSignalQlBelowThreshAlm alarm will be generated when 
             1PPS input signal  quality level of '1PPS+ToD' port is
             below the following threshold.
             locked(1): 1PPS input signal source is locked with grandmaster
                        at this time.
             qlprc(2): quality level of primary reference clock, it is defined 
                       in recommendation G.811. 1PPS input signal is in holdover
                       state at this time by using PRC.
             qlssua(4): quality level of type I/V slave clock, it is defined 
                       in recommendation G.812. 1PPS input signal is in holdover
                       state at this time by using SSU-A.
             qlssub(8): quality level of type VI slave clock, it is defined 
                       in recommendation G.812. 1PPS input signal is in holdover
                       state at this time by using SSU-B.             
             qlsec(11): quality level of SDH equipment clock, it is defined 
                       in recommendation G.813. 1PPS input signal is in holdover
                       state at this time by using SEC."                             
        DEFVAL { qlprc }
        ::= {zxAnIeee1588AlmThreshObjects 3}

