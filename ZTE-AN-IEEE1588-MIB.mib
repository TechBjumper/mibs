        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The PTP priority 2.
             The value 0 indicates the highest priority."
        REFERENCE
            "IEEE Standard for a Precision Clock Synchronization Protocol for
             Networked Measurement and Control Systems 8.2.1.4.2 
             defaultDS.priority2"
        DEFVAL { 128 }                           
        ::= { zxAnPtpPrfEntry 3 }
                
    zxAnPtpPrfDomainNumber   OBJECT-TYPE
        SYNTAX      Integer32(24..43)
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The PTP domain number."
        REFERENCE
            "IEEE Standard for a Precision Clock Synchronization Protocol for
             Networked Measurement and Control Systems 8.2.1.4.3 
             defaultDS.domainNumber"
        DEFVAL { 24 }                            
        ::= { zxAnPtpPrfEntry 4 }       

    zxAnPtpPrfLocalPriority   OBJECT-TYPE
        SYNTAX      Integer32(1..255)
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The PTP local priority.
             The value 1 indicates the highest priority."
        REFERENCE
            "G.8275.1 6.3.2"
        DEFVAL { 128 }                           
        ::= { zxAnPtpPrfEntry 5 }
                
    zxAnPtpPrfMaxStepsRemoved   OBJECT-TYPE
        SYNTAX      Integer32(1..255)
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The PTP maximum steps removed.
             The parameter is defined in [IEEE1588] is used to indicate the
             distance between a clock and the grandmaster clock in the network. 
             It was originally intended to be used as a helpful tool to detect
             cyclic paths that may occur in PTP systems. By default it is 
             required that a clock not qualify any Announce messages where the
             stepsRemoved field is equal to, or greater than, 255 in order to
             ensure that rogue frames are extinguished."
        REFERENCE
            "G.8275.1 Annex F"
        DEFVAL { 255 }                           
        ::= { zxAnPtpPrfEntry 6 }

