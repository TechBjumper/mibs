    zxAnSyncTime1ppsSrcTable   OBJECT-TYPE
        SYNTAX      SEQUENCE  OF  ZxAnSyncTime1ppsSrcEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION "This table includes 1PPS source information in NE 
                    equipment."
        ::=  { zxAnIeee15881ppsObjects 3 }

    zxAnSyncTime1ppsSrcEntry   OBJECT-TYPE
        SYNTAX      ZxAnSyncTime1ppsSrcEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION 
            "An entry in zxAnSyncTime1ppsSrcTable.
            to create a 1pps source,it must comply with the multi-variables
            usage rule - all the mib variables related to same fuction 
            must be in one snmp set operation.
            the variables have two type mandatory and optional:
            Mandatory variables must be in the one snmp set operation.
            Optional variables may be in the one snmp set operation with the
            mandatory variables, but must not be used alone.
            
            case 1 - to create 1PPS signal source from 1PPS port:
            -Mandatory:
            zxAnSyncTime1ppsSrcRack,
            zxAnSyncTime1ppsSrcShelf,
            zxAnSyncTime1ppsSrcSlot,
            zxAnSyncTime1ppsSrcSignalType,
            zxAnSyncTime1ppsSrcPortType,
            zxAnSyncTime1ppsSrcPort,
            zxAnSyncTime1ppsSrcPriority, 
            zxAnSyncTime1ppsSrcRowStatus,
            -Optional: 
            
            case 3 - to destroy 1PPS signal
            -Mandatory:
            zxAnSyncTime1ppsSrcRowStatus,"
        INDEX   {zxAnSyncTime1ppsSrcId}
        ::=  { zxAnSyncTime1ppsSrcTable 1 }

    ZxAnSyncTime1ppsSrcEntry  ::=  SEQUENCE {         
        zxAnSyncTime1ppsSrcId                  Integer32,
        zxAnSyncTime1ppsSrcRack                Integer32,
        zxAnSyncTime1ppsSrcShelf               Integer32,
        zxAnSyncTime1ppsSrcSlot                Integer32,
        zxAnSyncTime1ppsSrcFirstPtp            TruthValue,
        zxAnSyncTime1ppsSrcSignalType          INTEGER,
        zxAnSyncTime1ppsSrcPriority            Integer32,
        zxAnSyncTime1ppsSrcPortType            INTEGER,
        zxAnSyncTime1ppsSrcPort                Integer32,
        zxAnSyncTime1ppsSrcWorkStatus          INTEGER,
        zxAnSyncTime1ppsSrcValidStatus         INTEGER,
        zxAnSyncTime1ppsSrcPortStatus          INTEGER,                
        zxAnSyncTime1ppsSrcFormat              INTEGER,
        zxAnSyncTime1ppsSrcCurrentQL           INTEGER,
        zxAnSyncTime1ppsSrcTimeAccuracy        Integer32,
        zxAnSyncTime1ppsSrcTimeSrcType         INTEGER,
        zxAnSyncTime1ppsSrcDlyCompensate       Integer32,
        zxAnSyncTime1ppsSrcRowStatus           RowStatus        
        }

    zxAnSyncTime1ppsSrcId  OBJECT-TYPE
        SYNTAX      Integer32(1..255)       
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This '1PPS+ToD' source id."
        ::= { zxAnSyncTime1ppsSrcEntry 1 }   
             
    zxAnSyncTime1ppsSrcRack  OBJECT-TYPE
        SYNTAX      Integer32(1)        
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This object indicates the rack number."
        ::= { zxAnSyncTime1ppsSrcEntry 2 }   
          
    zxAnSyncTime1ppsSrcShelf  OBJECT-TYPE
        SYNTAX      Integer32(1)        
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This object indicates the shelf number."
        ::= { zxAnSyncTime1ppsSrcEntry 3 }         
        
    zxAnSyncTime1ppsSrcSlot  OBJECT-TYPE
        SYNTAX      Integer32(1..22)        
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This object indicates the slot number.
            Only support time card, such as cick."
        ::= { zxAnSyncTime1ppsSrcEntry 4 }     
        
   
    zxAnSyncTime1ppsSrcFirstPtp  OBJECT-TYPE
        SYNTAX      TruthValue        
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The PTP node provided '1PPS+ToD' siginal is the first node of
            this slot or not."
        DEFVAL { true }
        ::= { zxAnSyncTime1ppsSrcEntry 5 }          
        
    zxAnSyncTime1ppsSrcSignalType      OBJECT-TYPE 
        SYNTAX          INTEGER
        {   
             ptp(1),
             onepps(2)
        }
        MAX-ACCESS      read-create
        STATUS          current
        DESCRIPTION     
            "The type of '1PPS+ToD' signal source.
            ptp(1)    : 1PPS come from PTP
