-- 04. zxAnIeee1588TimeStatusInfoObjs
-------------------------------------------------------------------------------
    zxAn1588TimeAccuracyDegradAlmRsn  OBJECT-TYPE
        SYNTAX      INTEGER {
            freqLol(1),
            freqQualityLevelBelowThresh(2)            
        }
        MAX-ACCESS  accessible-for-notify
        STATUS      current
        DESCRIPTION
            "The time accuracy degraded alarm reason.
             A zxAn1588TimeAccuracyDegradeAlm alarm will be generated because 
             for the following threshold.
             freqLol(1): frequency loss of lock.
             freqQualityLevelBelowThresh(2): frequency quality level below
                  threshold."                     
        ::= {zxAnIeee1588TimeStatusInfoObjs 1}
        
    zxAn1588TimeSourceType  OBJECT-TYPE
        SYNTAX   INTEGER{
            none(1),
            ptp(2),
            oneppsPlusTod(3)
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The current time source type.            
             none(1): no time source.
             ptp(2): PTP time source.
             oneppsPlusTod: 1PPS+ToD time source."
        ::= {zxAnIeee1588TimeStatusInfoObjs 2}

    zxAn1588TimeSourceId  OBJECT-TYPE
        SYNTAX      DisplayString(SIZE(1..23))
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The current time source identification.
             It indicates the PTP time source ID when zxAn1588TimeSourceType is 
             ptp(1). It indicates the 1PPS+ToD time source ID when 
             zxAn1588TimeSourceType is oneppsPlusTod(2). 
             If zxAn1588TimeSourceType is none(1), the attribute value is 
             empty string."
        ::= {zxAnIeee1588TimeStatusInfoObjs 3}

    zxAn1588LocalTimeStatus  OBJECT-TYPE
        SYNTAX      INTEGER{
            freeRun(1),
            synchronizing(2),
            locked(3),
            holdoverInSpecification(4),
            holdoverOutOfSpecification(5),
            holdover(6)
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The local time status.
             freeRun(1): the local time has never been synchronized with any 
                       time source.
             synchronizing(2): the local time is synchronizing with a time
                       source.
             locked(3): the local time has been synchronized with a time source.
             holdoverInspecification(4): the local time is no longer 
                       synchronized with a time source and is using information 
                       obtained while it was previously synchronized or other 
                       information sources were still available, to maintain 
                       performance within desired specification. 
                       It is defined in recommendation G.8275.1 appendix V.
             holdoverOutOfSpecification(5): the local time is no longer 
                       synchronized with a time source and, while it may be 
                       using information obtained while it was previously 
                       synchronized or other information sources were still 
                       available, it is unable to maintain performance 
                       within desired specification.
                       It is defined in recommendation G.8275.1 appendix V.
             holdover(6): the local time is no longer synchronized with a time
                       source, this value is only used for 
                       zxAn1588LocalTimeStatusChanged trap."
        ::= {zxAnIeee1588TimeStatusInfoObjs 4}

    zxAn1588LocalClockFreqStatus  OBJECT-TYPE
        SYNTAX      INTEGER{
            freeRun(1),
            locked(2),
            holdover(3)
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The local clock frequency status.            
             freerun(1): the local clock frequency is in free-run state.
             locked(2): the local clock frequency is in locked state.
             holdover(3): the local clock frequency is in holdover state."
        ::= {zxAnIeee1588TimeStatusInfoObjs 5}

    zxAn1588LocalClockFreqQltyLvl  OBJECT-TYPE
        SYNTAX      INTEGER{
            qlprc(2),
            qlssua(4),
            qlssub(8),
            qlsec(11)
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The local clock frequency quality level.            
             qlprc(2): quality level of primary reference clock, it is defined 
                       in recommendation G.811. 1PPS input signal is in holdover
                       state at this time by using PRC.
             qlssua(4): quality level of type I/V slave clock, it is defined 
                       in recommendation G.812. 
             qlssub(8): quality level of type VI slave clock, it is defined 
                       in recommendation G.812. 
             qlsec(11): quality level of SDH equipment clock, it is defined 
                       in recommendation G.813. "
        ::= {zxAnIeee1588TimeStatusInfoObjs 6}

    zxAn1588PtpMessageReceiveStatus  OBJECT-TYPE
        SYNTAX      INTEGER{
            normal(1),
            annocMessageMissed(2),
            syncMessageMissed(3),
            annocAndSyncMessagesMissed(4)            
