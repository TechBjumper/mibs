    zxAn1588LastLocalTimeStatus OBJECT-TYPE
        SYNTAX      INTEGER{
            freeRun(1),
            synchronizing(2),
            locked(3),
            holdoverInSpecification(4),
            holdoverOutOfSpecification(5),
