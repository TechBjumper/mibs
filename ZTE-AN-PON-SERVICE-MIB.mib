-- PON whitelist  
-----------------------------------------------------------      
    zxAnPonSrvWhitelistTable OBJECT-TYPE
        SYNTAX         SEQUENCE OF ZxAnPonSrvWhitelistEntry
        MAX-ACCESS     not-accessible
        STATUS         current
        DESCRIPTION
            "PON whitelist table."
        ::= { zxAnPonSrvWhitelistObjects 2}

    zxAnPonSrvWhitelistEntry   OBJECT-TYPE
        SYNTAX         ZxAnPonSrvWhitelistEntry
        MAX-ACCESS     not-accessible
        STATUS         current
        DESCRIPTION
            "PON whitelist entry."
        INDEX {zxAnPonSrvWhitelistType, zxAnPonSrvWhitelistRule}
        ::= { zxAnPonSrvWhitelistTable 1}

    ZxAnPonSrvWhitelistEntry ::=
        SEQUENCE {            
                 zxAnPonSrvWhitelistType            INTEGER,
                 zxAnPonSrvWhitelistRule            DisplayString,
                 zxAnPonSrvWhitelistRowStatus       RowStatus
                 }       
                 
    zxAnPonSrvWhitelistType  OBJECT-TYPE
        SYNTAX      INTEGER {
                    eponMac(1),
                    gponSn(2)
                    }
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
             "Whitelist type."
        ::= { zxAnPonSrvWhitelistEntry   1 }

     zxAnPonSrvWhitelistRule OBJECT-TYPE
        SYNTAX      DisplayString(SIZE(1..128))
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "PON whitelist rule. Whitelist rule can be a specified 
             SN/MAC or SN/MAC range(GPON uses SN, EPON uses MAC). 
             '*' or '[begin-end]' can be used in SN/MAC string to 
             represent a range. Each '*' or '[begin-end]' represents
             only one legal character of the whiltelist rule. '*' 
             represents all legal characters, in MAC it represents '0' 
             to 'f', in SN it represents '0' to '9'. For example:
             GPON SN 
               ZTEG0001000[1-9] represents ZTEG00010001 to ZTEG00010009; 
               ZTEG0001000* represents ZTEG00010000 to ZTEG00010009;
               ZTEG000100** represents ZTEG00010000 to ZTEG00010099;
             EPON MAC
               00d0d02505** represents 00d0d0250500 to 00d0d02505ff.
               00d0d02505[1-7]* represents 00d0d0250510 to 00d0d025057f."
        ::= { zxAnPonSrvWhitelistEntry   2 }
                
    zxAnPonSrvWhitelistRowStatus           OBJECT-TYPE
        SYNTAX      RowStatus
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The status of this conceptual row.
             To create a row in this table, a manager must
             set this object to createAndGo(4).        
             To delete a row in this table, a manager must
             set this object to destroy(6). " 
        ::= { zxAnPonSrvWhitelistEntry   50}    

