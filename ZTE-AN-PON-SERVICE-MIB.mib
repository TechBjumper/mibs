-- PON ONU Separation Profile      
-- zxAnPonSrvOnuSepProfileTable
--------------------------------------------------------------------------------     
    zxAnPonSrvOnuSepProfileTable OBJECT-TYPE
        SYNTAX     SEQUENCE OF ZxAnPonSrvOnuSepProfileEntry
        MAX-ACCESS not-accessible
        STATUS     current
        DESCRIPTION
            "PON ONU separation profile table."
        ::= { zxAnPonSrvOnuProfileObjects 4 }
        
    zxAnPonSrvOnuSepProfileEntry OBJECT-TYPE
        SYNTAX      ZxAnPonSrvOnuSepProfileEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "PON ONU separation profile Entry."
        INDEX   { zxAnPonSrvOnuSepPrfPonType,zxAnPonSrvOnuSepPrfType,
                  zxAnPonSrvOnuSepPrfName }
        ::= { zxAnPonSrvOnuSepProfileTable 1 }
        
    ZxAnPonSrvOnuSepProfileEntry ::=
        SEQUENCE {
            zxAnPonSrvOnuSepPrfPonType              INTEGER,
            zxAnPonSrvOnuSepPrfType                 INTEGER,
            zxAnPonSrvOnuSepPrfName                 DisplayString,
            zxAnPonSrvOnuSepPrfId                   Integer32,
            zxAnPonSrvOnuSepPrfOperation            INTEGER,
            zxAnPonSrvOnuSepPrfStatus               INTEGER,
            zxAnPonSrvOnuSepPrfOperProgress         Integer32,
            zxAnPonSrvOnuSepPrfRowStatus            RowStatus
        }

    zxAnPonSrvOnuSepPrfPonType OBJECT-TYPE
        SYNTAX      INTEGER {
                    epon(1),
                    gpon(2)
                    }
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
                "PON type."
        DEFVAL { gpon }
        ::= { zxAnPonSrvOnuSepProfileEntry 1 }
        
    zxAnPonSrvOnuSepPrfType OBJECT-TYPE
        SYNTAX      INTEGER {
                    linePrf(1),
                    onuRemotePrf(2)
                    }
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
                "PON ONU separation profile type."       
        ::= { zxAnPonSrvOnuSepProfileEntry 2 }

    zxAnPonSrvOnuSepPrfName OBJECT-TYPE
        SYNTAX      DisplayString(SIZE(1..64))
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "PON ONU separation profile name."
        ::= { zxAnPonSrvOnuSepProfileEntry 3 }
        
    zxAnPonSrvOnuSepPrfId OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "PON ONU separation profile ID."
        ::= { zxAnPonSrvOnuSepProfileEntry 4 }
        
    zxAnPonSrvOnuSepPrfOperation OBJECT-TYPE
        SYNTAX      INTEGER {
                    commit(1),
                    rollback(2)
                    }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "PON ONU separation profile operation.           
            -commit: commit ONU separation profile's modification.
            -rollback: revert ONU separation profile's modification. "
        ::= { zxAnPonSrvOnuSepProfileEntry 5 }
        
    zxAnPonSrvOnuSepPrfStatus OBJECT-TYPE
        SYNTAX      INTEGER {
                    normal(1),
                    modifiedButNotApplied(2),
                    committing(3),
                    rollbacking(4)
                    }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The current status of PON ONU separation profile.
            -normal: profile has been applied.
            -modifiedButNotApplied: profile has been modified but not applied.
            -committing: profile is committing.
            -rollbacking: profile is rollbacking."
        ::= { zxAnPonSrvOnuSepProfileEntry 6 }
        
    zxAnPonSrvOnuSepPrfOperProgress OBJECT-TYPE
        SYNTAX      Integer32(0..100)
        UNITS       "percent"
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The percent of PON ONU separation profile operation."
        ::= { zxAnPonSrvOnuSepProfileEntry 7 }
        
    zxAnPonSrvOnuSepPrfRowStatus OBJECT-TYPE
        SYNTAX      RowStatus
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
           "The status of this conceptual row.

           The supported actions of this conceptual row only include:

           To create a row in this table, a manager must
           set this object to createAndGo(4).

           To delete a row in this table, a manager must
           set this object to destroy(6).

           To query this object, always return 1. "
        ::= { zxAnPonSrvOnuSepProfileEntry 50 }

