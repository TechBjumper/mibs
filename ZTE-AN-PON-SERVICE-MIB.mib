-- zxAnPonSrvVndBindOnuRuleTable
-------------------------------------------------------------------------------
   zxAnPonSrvVndBindOnuRuleTable OBJECT-TYPE
        SYNTAX     SEQUENCE OF ZxAnPonSrvVndBindOnuRuleEntry 
        MAX-ACCESS not-accessible
        STATUS     current
        DESCRIPTION
            "The VND binding ONU rule table."
        ::= { zxAnPonSrvVndObjects 4 }
        
   zxAnPonSrvVndBindOnuRuleEntry OBJECT-TYPE
        SYNTAX      ZxAnPonSrvVndBindOnuRuleEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The VND binding ONU rule entry."
        INDEX   { zxAnPonSrvVndName, zxAnPonSrvVndBindOnuRuleId }
        ::= { zxAnPonSrvVndBindOnuRuleTable 1 }

    ZxAnPonSrvVndBindOnuRuleEntry ::=
        SEQUENCE {
            zxAnPonSrvVndBindOnuRuleId           Integer32,
            zxAnPonSrvVndBindOnuType             INTEGER,
            zxAnPonSrvVndBindOnuStartMac         MacAddress,
            zxAnPonSrvVndBindOnuEndMac           MacAddress,
            zxAnPonSrvVndBindOnuSn               OCTET STRING,
            zxAnPonSrvVndBindOnuRegExpRule       DisplayString,
            zxAnPonSrvVndBindOnuLoid             DisplayString,
            zxAnPonSrvVndBindOnuRuleType         INTEGER,
            zxAnPonSrvVndBindOnuPwd              OCTET STRING,
            zxAnPonSrvVndBindOnuPsk              OCTET STRING,
            zxAnPonSrvVndBindOnuRid              DisplayString, 
            zxAnPonSrvVndBindOnuLoidPwd          OCTET STRING,
            zxAnPonSrvVndBindOnuRowStatus        RowStatus
        }
        
    zxAnPonSrvVndBindOnuRuleId OBJECT-TYPE
        SYNTAX      Integer32(1..65535)
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The rule ID of the VND binding ONU."
        ::= { zxAnPonSrvVndBindOnuRuleEntry 1 }   
           
    zxAnPonSrvVndBindOnuType OBJECT-TYPE
        SYNTAX      INTEGER{
            gpon(1),
            epon(2)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The type of the VND binding ONU."
        ::= { zxAnPonSrvVndBindOnuRuleEntry 2 }
                   
    zxAnPonSrvVndBindOnuStartMac OBJECT-TYPE
        SYNTAX      MacAddress
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The starting MAC address of the VND binding ONU."
        ::= { zxAnPonSrvVndBindOnuRuleEntry 3 } 
        
    zxAnPonSrvVndBindOnuEndMac OBJECT-TYPE
        SYNTAX      MacAddress
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The ending MAC address of the VND binding ONU."
        ::= { zxAnPonSrvVndBindOnuRuleEntry 4} 

    zxAnPonSrvVndBindOnuSn OBJECT-TYPE
        SYNTAX      OCTET STRING(SIZE (8))
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The serial number of the VND binding ONU.
             The value '0x0000000000000000' indicates this attribute 
             is invalid."
        ::= { zxAnPonSrvVndBindOnuRuleEntry 5 }
        
     zxAnPonSrvVndBindOnuRegExpRule OBJECT-TYPE
        SYNTAX      DisplayString(SIZE(0..128))
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The regular expression rule of the VND binding ONU.
             For example:
             GPON SN 
               ZTEG0001000[1-9] represents ZTEG00010001 to ZTEG00010009; 
               ZTEG0001000* represents ZTEG00010000 to ZTEG00010009;
               ZTEG000100** represents ZTEG00010000 to ZTEG00010099;
             EPON MAC
               00d0d02505** represents 00d0d0250500 to 00d0d02505ff;
               00d0d02505[1-7]* represents 00d0d0250510 to 00d0d025057f.
             The empty string indicates this attribute is invalid."
        ::= { zxAnPonSrvVndBindOnuRuleEntry 6}
    
    zxAnPonSrvVndBindOnuLoid OBJECT-TYPE
        SYNTAX      DisplayString(SIZE (0..24))
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The LOID of the VND binding ONU.
             The empty string indicates this attribute is invalid."      
        ::= { zxAnPonSrvVndBindOnuRuleEntry 7}
        
    zxAnPonSrvVndBindOnuRuleType OBJECT-TYPE
        SYNTAX      INTEGER{
            mac(1),
            macRange(2),
            macRegex(3),
            sn(4),
            snRegex(5),
            snPlusPwd(6),
            snPlusRid(7),
            pwd(8),
            loid(9),
            loidPlusPwd(10),
            psk(11),
            rid(12)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The rule type of the VND binding ONU.
             mac(1) : MAC address rule, zxAnPonSrvVndBindOnuStartMac is valid.
             macRange(2) : MAC address range rule, zxAnPonSrvVndBindOnuStartMac 
                           and zxAnPonSrvVndBindOnuEndMac are valid.
             macRegex(3) : MAC address regex rule, 
                           zxAnPonSrvVndBindOnuRegExpRule is valid.
             sn(4) : SN rule, zxAnPonSrvVndBindOnuSn is valid.
             snRegex(5) : SN regular expression rule, 
                          zxAnPonSrvVndBindOnuRegExpRule is valid.                       
             snPlusPwd(6) : SN + password rule, zxAnPonSrvVndBindOnuSn and
                            zxAnPonSrvVndBindOnuPwd are valid.
             snPlusRid(7) : SN + RID rule, zxAnPonSrvVndBindOnuSn and
                            zxAnPonSrvVndBindOnuRid are valid.
             pwd(8) : password rule, zxAnPonSrvVndBindOnuPwd is valid.
             loid(9) : LOID rule, zxAnPonSrvVndBindOnuLoid is valid.
             loidPlusPwd(10) : LOID + password rule, 
                               zxAnPonSrvVndBindOnuLoid and
                               zxAnPonSrvVndBindOnuLoidPwd are valid.
             psk(11) : PSK rule, zxAnPonSrvVndBindOnuPsk is valid.
             rid(12) : RID rule, zxAnPonSrvVndBindOnuRid is valid."                        
        ::= { zxAnPonSrvVndBindOnuRuleEntry 8}          
        
    zxAnPonSrvVndBindOnuPwd OBJECT-TYPE
        SYNTAX      OCTET STRING(SIZE(0..10))
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The ONU password of the VND binding ONU.
             The value '0x00' indicates this attribute is invalid."
        ::= { zxAnPonSrvVndBindOnuRuleEntry 9}        
        
    zxAnPonSrvVndBindOnuPsk OBJECT-TYPE
        SYNTAX      OCTET STRING(SIZE (16))
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The PSK value of the VND binding ONU.
             This attribute value is hex data.
             The value '0x00000000000000000000000000000000' 
             indicates this attribute is invalid."
        ::= { zxAnPonSrvVndBindOnuRuleEntry 10}        
        
    zxAnPonSrvVndBindOnuRid OBJECT-TYPE
        SYNTAX      DisplayString(SIZE(0..36))
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The registration ID of the VND binding ONU.
             The empty string indicates this attribute is invalid."
        ::= { zxAnPonSrvVndBindOnuRuleEntry 11}         
        
    zxAnPonSrvVndBindOnuLoidPwd OBJECT-TYPE
        SYNTAX      OCTET STRING(SIZE(0..12))
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The LOID password of the VND binding ONU.
             The value '0x00' indicates this attribute is invalid."
        ::= { zxAnPonSrvVndBindOnuRuleEntry 12}        
            
    zxAnPonSrvVndBindOnuRowStatus OBJECT-TYPE
        SYNTAX      RowStatus
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The status of this conceptual row.
             The state of this conceptual row only support
             'not exist' and  'active'.
             The action of this conceptual row only support
             'createAndGo' and 'destroy'.

             To create a row in this table, a manager must
             set this object to createAndGo(4).

             To delete a row in this table, a manager must
             set this object to destroy(6).

             To query this object, always return 1. "
        ::= { zxAnPonSrvVndBindOnuRuleEntry 50 }

