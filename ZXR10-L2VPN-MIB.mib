--********************************************    
--*************zxr10L2vpnGlobalPwPara*************** 
zxr10L2vpnGlobalPWCapatity OBJECT-TYPE
            SYNTAX      Unsigned32(0..4294967295)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION 
                " The summary information about max_num of PWs'configuration."
            ::= { zxr10l2vpnGlobalPara 1 }   
                 
zxr10L2vpnGlobalPWUsed OBJECT-TYPE
            SYNTAX      Unsigned32(0..4294967295)    
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION 
                "The summary information about current_num of PWs'configuration."
            ::= { zxr10l2vpnGlobalPara 2 } 
                       
--*************zxr10L2vpnEnable*************** 
zxr10L2vpnEnable OBJECT-TYPE
            SYNTAX INTEGER {
            disable(0),
            enable(1)           
            }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION 
                "Enable L2VPN service."
            ::= { zxr10l2vpnObjects 20 }    
                       
--*************zxr10L2vpnSduTable*************** 
zxr10L2vpnSduTable OBJECT-TYPE
            SYNTAX     SEQUENCE OF Zxr10L2vpnSduEntry
            MAX-ACCESS not-accessible
            STATUS     current
            DESCRIPTION
                "Sdu Table. This table contains
                 one entry per sdu table."        
            ::= { zxr10l2vpnObjects 1 }

zxr10L2vpnSduEntry OBJECT-TYPE
            SYNTAX     Zxr10L2vpnSduEntry
            MAX-ACCESS not-accessible
            STATUS     current
            DESCRIPTION
                "The parameter of a Sdu entity containing "
            INDEX { zxr10L2vpnSduServiceName, zxr10L2vpnSduName }
            ::= { zxr10L2vpnSduTable 1 }

Zxr10L2vpnSduEntry ::= SEQUENCE {
            zxr10L2vpnSduServiceName                DisplayString,
            zxr10L2vpnSduName                       DisplayString,
            zxr10L2vpnSduServiceType                INTEGER,
            zxr10L2vpnSduRedandancyMgrName          DisplayString,
            zxr10L2vpnSduRedundancyPrimaryPwPeer    IpAddress,
            zxr10L2vpnSduRedundancyPrimaryPwVcid    Unsigned32,
            zxr10L2vpnSduRedundancyPrimaryPwVcType  L2vpnPWType,
            zxr10L2vpnSduRedundancyBackupPwPeer     IpAddress,
            zxr10L2vpnSduRedundancyBackupPwVcid     Unsigned32,
            zxr10L2vpnSduRedundancyBackupPwVcType   L2vpnPWType
            }

zxr10L2vpnSduServiceName OBJECT-TYPE
            SYNTAX       DisplayString
            MAX-ACCESS   read-only
            STATUS       current
            DESCRIPTION 
                "The name of the service instance.It consists 
                of 'Forwarder' modules connected by pseudowires
                 and Attach Circuit."
            ::= { zxr10L2vpnSduEntry 1 }

zxr10L2vpnSduName OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION 
                "The name of Service Distribution Unit (SDU)."
            ::= { zxr10L2vpnSduEntry 2 }

zxr10L2vpnSduServiceType OBJECT-TYPE
            SYNTAX INTEGER {
            unknown(0),
            vpls(1),
            vpws(2),
            mspw(3),
            enhanced-vpws(6)            
            }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION 
                "The type of service instance.There are several 
                 types of L2VPNs:(1)Virtual Private LAN Service(VPLS),
                 and (2)Ethernet over PW,and (3)Multi-segment Pseudowire(MSPW),
                 and (4)Enhanced-Ethernet over Pseudowire"
            ::= { zxr10L2vpnSduEntry 3 }
            
zxr10L2vpnSduRedandancyMgrName OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "The name of  Redundency-manager."
            ::= { zxr10L2vpnSduEntry 4 } 
 
zxr10L2vpnSduRedundancyPrimaryPwPeer OBJECT-TYPE
            SYNTAX      IpAddress
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "The PW's remote endpoint address."
            ::= { zxr10L2vpnSduEntry 5 }
            
zxr10L2vpnSduRedundancyPrimaryPwVcid OBJECT-TYPE
            SYNTAX      Unsigned32(0..4294967295)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "A non-zero 32-bit connection ID that, together 
                 with the PW type, identifies a particular PW.  
                 Note that the PW ID and the PW type MUST be the 
                 same at both endpoints."
            ::= { zxr10L2vpnSduEntry 6 }  
            
zxr10L2vpnSduRedundancyPrimaryPwVcType OBJECT-TYPE
            SYNTAX L2vpnPWType
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "A 15-bit quantity containing a value that represents 
                 the type of PW. Assigned values are specified in 'IANA 
                 Allocations for Pseudowire Edge to Edge Emulation (PWE3)'."
            ::= { zxr10L2vpnSduEntry 7 }  
            
zxr10L2vpnSduRedundancyBackupPwPeer OBJECT-TYPE
            SYNTAX      IpAddress
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "The PW's remote endpoint address."
            ::= { zxr10L2vpnSduEntry 8 }

zxr10L2vpnSduRedundancyBackupPwVcid OBJECT-TYPE
            SYNTAX      Unsigned32(0..4294967295)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "A non-zero 32-bit connection ID that, together with the PW type, 
                 identifies a particular PW.  Note that the PW ID and the PW type 
                 MUST be the same at both endpoints."
            ::= { zxr10L2vpnSduEntry 9 }

zxr10L2vpnSduRedundancyBackupPwVcType OBJECT-TYPE
            SYNTAX L2vpnPWType
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "A 15-bit quantity containing a value that represents the type of PW.
                 Assigned values are specified in 'IANA Allocations for Pseudowire Edge 
                 to Edge Emulation (PWE3)'."
            ::= { zxr10L2vpnSduEntry 10 }

--*************zxr10L2vpnSacTable*************** 
zxr10L2vpnSacTable OBJECT-TYPE
            SYNTAX     SEQUENCE OF Zxr10L2vpnSacEntry
            MAX-ACCESS not-accessible
            STATUS     current
            DESCRIPTION
                "Sac Table. This table contains
                 one entry per Sac table."        
            ::= { zxr10l2vpnObjects 2 }

zxr10L2vpnSacEntry OBJECT-TYPE
            SYNTAX     Zxr10L2vpnSacEntry
            MAX-ACCESS not-accessible
            STATUS     current
            DESCRIPTION
                "The parameter of a Sac entity containing "
            INDEX { zxr10L2vpnSacServiceName, zxr10L2vpnSacName }
            ::= { zxr10L2vpnSacTable 1 }

Zxr10L2vpnSacEntry ::= SEQUENCE {
            zxr10L2vpnSacServiceName                DisplayString,
            zxr10L2vpnSacName                       DisplayString,
            zxr10L2vpnSacServiceType                INTEGER,
            zxr10L2vpnSacServiceDefine              INTEGER,
            zxr10L2vpnSacEthSwitchSplitHorizone     INTEGER,            
            zxr10L2vpnSacEthIngressAdjustType       INTEGER,
            zxr10L2vpnSacEthPushedTag               Integer32,
            zxr10L2vpnSacEthRewriteTargetTag        Integer32,
            zxr10L2vpnSacTdmJitterBuffer            Integer32,
            zxr10L2vpnSacTdmDistributePeriod        Integer32,
            zxr10L2vpnSacTdmTimeslots               Unsigned32,
            zxr10L2vpnSacTdmStructureType           INTEGER,
            zxr10L2vpnSacEthSwitchGroupID           Integer32,
	    zxr10L2vpnSacInterworking               INTEGER,
	    zxr10L2vpnSacInterworkingIpLocalCeMac   DisplayString
            }

zxr10L2vpnSacServiceName OBJECT-TYPE
            SYNTAX       DisplayString
            MAX-ACCESS   read-only
            STATUS       current
            DESCRIPTION 
                "The name of the service instance.It consists of 
                'Forwarder' modules connected by pseudowires and Attach Circuit."
            ::= { zxr10L2vpnSacEntry 1 }

zxr10L2vpnSacName OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION 
                "The name of Service Attachment Circuit(SAC)."
            ::= { zxr10L2vpnSacEntry 2 }

zxr10L2vpnSacServiceType OBJECT-TYPE
            SYNTAX INTEGER {
            unknown(0),
            vpls(1),
            vpws(2),
            vlss(4),
            enhanced-vpws(6)            
            }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION 
                "The type of service instance.There are several 
                types of L2VPNs:(1)Virtual Private LAN Service(VPLS),
                and (2)Virtual Private Wire Service(VPWS),and 
                (3)Virtual Local Switching Service(VLSS),and 
                (4)Enhanced-Virtual Private Wire Service(Enhanced-VPWS) ;"
            ::= { zxr10L2vpnSacEntry 3 }

zxr10L2vpnSacServiceDefine OBJECT-TYPE
            SYNTAX INTEGER { 
            ethernt(1),
            tdm(2),
            ppp(3),
            hdlc(4),
            fr(5)
            }
            MAX-ACCESS 	read-only
            STATUS     	current
            DESCRIPTION
                 "The parameter of service accesse.There are several 
                 parameters of service accesse:(1)ethernt,(2)tdm,
                 (3)ppp,(4)hdlc,(5)fr."
            ::= { zxr10L2vpnSacEntry 4 } 

zxr10L2vpnSacEthSwitchSplitHorizone OBJECT-TYPE
            SYNTAX INTEGER { 
            null(0),
            on(1),
            off(2)
            }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "Split-horizon attribute which used in E-tree service 
                 includes two types client/server "
            ::= { zxr10L2vpnSacEntry 5 }

zxr10L2vpnSacEthIngressAdjustType OBJECT-TYPE
            SYNTAX INTEGER {
            unknown(0),
            needless(1),
            push(2),
            pop-outermost(3),
            rewrite(4)
            }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "The Type of VLAN tag ingress-adjustment.It consists 
                 of (1)needless,(2)push,(3)pop-outermost,(4)rewrite"
            ::= { zxr10L2vpnSacEntry 20 }

zxr10L2vpnSacEthPushedTag OBJECT-TYPE
            SYNTAX      Integer32(0..4094)            
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "A service-delimiting tag which pushed onto the incoming packets."
            ::= { zxr10L2vpnSacEntry 21 } 
            
zxr10L2vpnSacEthRewriteTargetTag OBJECT-TYPE
            SYNTAX      Integer32(1..4094)            
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "A VLAN tag which rewirted to replace the service-delimiting tags."
            ::= { zxr10L2vpnSacEntry 22 }

zxr10L2vpnSacTdmJitterBuffer OBJECT-TYPE
            SYNTAX      Integer32(2..64)            
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "The jitter-buffer size of TDM emulation."
            ::= { zxr10L2vpnSacEntry 23 }   

zxr10L2vpnSacTdmDistributePeriod OBJECT-TYPE
            SYNTAX      Integer32(1..64)            
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "Tthe period of distributing TDM packets to remote end-point."
            ::= { zxr10L2vpnSacEntry 24 } 

zxr10L2vpnSacTdmTimeslots OBJECT-TYPE
            SYNTAX      Unsigned32(0..4294967295)            
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "The time of TDM slots."
            ::= { zxr10L2vpnSacEntry 25 }                                      

zxr10L2vpnSacTdmStructureType OBJECT-TYPE
            SYNTAX INTEGER {
            aware(1),
            agnostic(2),
            aware-with-cas(3)
            }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "The structure type of TDM emulation."
            ::= { zxr10L2vpnSacEntry 26 }   

zxr10L2vpnSacEthSwitchGroupID OBJECT-TYPE
            SYNTAX      Integer32(0..64)            
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "Split-horizon-Group attribute. "
            ::= { zxr10L2vpnSacEntry 27 }    
            
zxr10L2vpnSacInterworking OBJECT-TYPE
            SYNTAX       INTEGER{ 
            none(0),
            ip(1),
            ethernet(2)
            }            
            MAX-ACCESS   read-only
            STATUS       current
            DESCRIPTION 
                "The name of SacInterworking."
            ::= { zxr10L2vpnSacEntry 28 }    

zxr10L2vpnSacInterworkingIpLocalCeMac OBJECT-TYPE
            SYNTAX       DisplayString
            MAX-ACCESS   read-only
            STATUS       current
            DESCRIPTION 
                "The LocalCeMac of SacInterworking."
            ::= { zxr10L2vpnSacEntry 29 } 			
			
--*************zxr10L2vpnRedundancyTable*************** 
zxr10L2vpnRedundancyTable OBJECT-TYPE
            SYNTAX     SEQUENCE OF Zxr10L2vpnRedundancyEntry
            MAX-ACCESS not-accessible
            STATUS     current
            DESCRIPTION
                "Redundancy Table. This table contains
                 one entry per Redundancy table."        
            ::= { zxr10l2vpnObjects 3 }

zxr10L2vpnRedundancyEntry OBJECT-TYPE
            SYNTAX     Zxr10L2vpnRedundancyEntry
            MAX-ACCESS not-accessible
            STATUS     current
            DESCRIPTION
                "The parameter of a Redundancy entity containing "
            INDEX { zxr10L2vpnRedundancyName }
            ::= { zxr10L2vpnRedundancyTable 1 }

Zxr10L2vpnRedundancyEntry ::= SEQUENCE { 
            zxr10L2vpnRedundancyName               DisplayString,
            zxr10L2vpnRedundancySduName            DisplayString,
            zxr10L2vpnRedundancyProtectMode        INTEGER,
            zxr10L2vpnRedundancyPfsNegoMode        INTEGER,
            zxr10L2vpnRedundancyProtectPolicy      INTEGER,
            zxr10L2vpnRedundancyProtectDelayTime   Integer32,
            zxr10L2vpnRedundancyRevertPolicy       INTEGER,
            zxr10L2vpnRedundancyRevertDelayTime    Integer32,
            zxr10L2vpnRedundancyActivePWs          INTEGER,
            zxr10L2vpnRedundancyStandbyPWs         INTEGER,
            zxr10L2vpnRedundancyInvalidPWs         INTEGER,
            zxr10L2vpnRedundancyProtectionCount    Counter32,
            zxr10L2vpnRedundancyLastProtectionTime Unsigned32
            }

zxr10L2vpnRedundancyName OBJECT-TYPE
            SYNTAX       DisplayString
            MAX-ACCESS   read-only
            STATUS       current
            DESCRIPTION 
                "The name of redundency-management."
            ::= { zxr10L2vpnRedundancyEntry 1 }

zxr10L2vpnRedundancySduName OBJECT-TYPE
            SYNTAX      DisplayString
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION 
                "The name of Service Distribution Unit (SDU) 
                belongs to the exact redundency-management instance."
            ::= { zxr10L2vpnRedundancyEntry 2 }

zxr10L2vpnRedundancyProtectMode OBJECT-TYPE
            SYNTAX INTEGER {
            oneByOne(1),
            onePlusOne(2)
            }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION 
                "The protectmode of redundency-management instance. 
                 There are two modes: (1)oneByOne, (2)onePlusOne"
            ::= { zxr10L2vpnRedundancyEntry 3 }
            
zxr10L2vpnRedundancyPfsNegoMode OBJECT-TYPE
            SYNTAX INTEGER {
            unsupported(1),
            independent(2),
            master(3),
            slave(4)
            }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "Configure the negotiation mode which include four 
                 modes :(1) unsupported, (2)independent, (3)master, (4)slave."
            ::= { zxr10L2vpnRedundancyEntry 4 } 

zxr10L2vpnRedundancyProtectPolicy OBJECT-TYPE
            SYNTAX INTEGER {
            immediately(1),
            delayed(2)            
            }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "The protect-policy of transforming primary-pw 
                 into backup-pw.There are two policies:(1)immediately, (2)delayed."
            ::= { zxr10L2vpnRedundancyEntry 5 }

zxr10L2vpnRedundancyProtectDelayTime OBJECT-TYPE
            SYNTAX      Integer32(10..180)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "The time of transforming primary-pw  into backup-pw."
            ::= { zxr10L2vpnRedundancyEntry 6 }  

zxr10L2vpnRedundancyRevertPolicy OBJECT-TYPE
            SYNTAX INTEGER {
            immediately(1),
            delayed(2),
            never(3)
            }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "The protectpolicy of transforming primary-pw 
                 from backup-pw. There are two policies:(1)immediately, (2)delayed."
            ::= { zxr10L2vpnRedundancyEntry 7 }

zxr10L2vpnRedundancyRevertDelayTime OBJECT-TYPE
            SYNTAX      Integer32(10..180)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "The time of transforming primary-pw from backup-pw."
            ::= { zxr10L2vpnRedundancyEntry 8 }

zxr10L2vpnRedundancyActivePWs OBJECT-TYPE
            SYNTAX INTEGER {
            primary(1),
            backup(2),
            none(3)
            }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "The statues of current active-pws.There are 
                  three statues:(1)primary,(2)backup,(3)none."
            ::= { zxr10L2vpnRedundancyEntry 9 }

zxr10L2vpnRedundancyStandbyPWs OBJECT-TYPE
            SYNTAX INTEGER {
            primary(1),
            backup(2),
            none(3)
            }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "The statues of current standby-pws.There are 
                  three statues:(1)primary,(2)backup,(3)none."
            ::= { zxr10L2vpnRedundancyEntry 10 } 

zxr10L2vpnRedundancyInvalidPWs OBJECT-TYPE
            SYNTAX INTEGER {
            primary(1),
            backup(2),
            none(3)
            }
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "The statues of current invalid-pws.There are
                  three statues:(1)primary,(2)backup,(3)none."
            ::= { zxr10L2vpnRedundancyEntry 11 }
			
zxr10L2vpnRedundancyProtectionCount OBJECT-TYPE
            SYNTAX      Counter32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "The total  number of the pws switchover.
                 Transforming primary-pw into backup-pw or 
                 transforming primary-pw from backup-pw means one."
            ::= { zxr10L2vpnRedundancyEntry 12 }
			
zxr10L2vpnRedundancyLastProtectionTime OBJECT-TYPE
            SYNTAX      Unsigned32(0..4294967295)
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                 "The time of the last switchover, which 
                 ransform primary-pw into backup-pw or 
                 transform primary-pw from backup-pw."
            ::= { zxr10L2vpnRedundancyEntry 13 }

--*************zxr10L2vpnPwTable*************** 
zxr10L2vpnPwTable OBJECT-TYPE
            SYNTAX     SEQUENCE OF Zxr10L2vpnPwEntry
            MAX-ACCESS not-accessible
            STATUS     current
            DESCRIPTION
                "Pw Table. This table contains
                 one entry per pw table."        
            ::= { zxr10l2vpnObjects 4 }

zxr10L2vpnPwEntry OBJECT-TYPE
            SYNTAX     Zxr10L2vpnPwEntry
            MAX-ACCESS not-accessible
            STATUS     current
            DESCRIPTION
                "The parameter of a pw entity containing statistics 
                information about a particular PW."
            INDEX { zxr10L2vpnPwPeer, zxr10L2vpnPwVcid, zxr10L2vpnPwVcType }
            ::= { zxr10L2vpnPwTable 1 }

Zxr10L2vpnPwEntry ::= SEQUENCE {
            zxr10L2vpnPwPeer              IpAddress,
            zxr10L2vpnPwVcid              Unsigned32,
            zxr10L2vpnPwVcType            L2vpnPWType,
            zxr10L2vpnPwSduName           DisplayString,
            zxr10L2vpnPwServiceName       DisplayString,
            zxr10L2vpnPwServiceType       INTEGER,
            zxr10L2vpnPwIsMspwSegment     INTEGER,
            zxr10L2vpnPwLocalLabel        Integer32,
            zxr10L2vpnPwRemoteLabel       Integer32,
            zxr10L2vpnPwSignalType        INTEGER,	
        	zxr10L2vpnPwLocalGroupId	  Integer32,
        	zxr10L2vpnPwLocalCbit	      INTEGER,
        	zxr10L2vpnPwLocalMTU	      Integer32,
        	zxr10L2vpnPwLocalDescription  DisplayString,
        	zxr10L2vpnPwLocalStatusWord	  Unsigned32,
        	zxr10L2vpnPwLocalStatusSignalAbility INTEGER,  
            zxr10L2vpnPwRemoteGroupId     Integer32,
        	zxr10L2vpnPwRemoteCbit        INTEGER,
        	zxr10L2vpnPwRemoteMTU         Integer32,
        	zxr10L2vpnPwRemoteDescription DisplayString,
         	zxr10L2vpnPwRemoteStatusWord  Unsigned32,
        	zxr10L2vpnPwRemoteStatusSignalAbility INTEGER, 
        	zxr10L2vpnPwNegoedCbit        INTEGER,
        	zxr10L2vpnPwNegoedMTU         Integer32,
        	zxr10L2vpnPwNegoedStatusSignalAbility   INTEGER,
        	zxr10L2vpnPwSignalState       INTEGER,     
        	zxr10L2vpnPwVccvLocalParas    Unsigned32, 
        	zxr10L2vpnPwVccvRemoteParas   Unsigned32,
        	zxr10L2vpnPwVccvNegoedParas   Unsigned32,			
