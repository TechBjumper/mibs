ZXR10-L2VPN-MIB DEFINITIONS ::= BEGIN  
     
IMPORTS
       OBJECT-TYPE,MODULE-IDENTITY,             
       Integer32,Counter32,Unsigned32,enterprises,
       IpAddress
                                           FROM SNMPv2-SMI
       DisplayString,TEXTUAL-CONVENTION
                                           FROM SNMPv2-TC; 
             
zte MODULE-IDENTITY
         LAST-UPDATED "201005170000Z" 
         ORGANIZATION "ZTE Corporation"
         CONTACT-INFO
             "ZTE Corporation
              Nanjing Institute of ZTE Corporation

              No.68 Zijinghua Rd. Yuhuatai District, Nanjing, China

              Tel: +86-25-52870000"
			  
         DESCRIPTION
             "This MIB contains managed object definitions for the ZXROSng L2vpn"
			 
			 
         ::= { enterprises 3902 }      

      zxr10 OBJECT IDENTIFIER ::= { zte 3} 
      zxr10l2vpn OBJECT IDENTIFIER ::= { zxr10 104}  
      zxr10l2vpnObjects OBJECT IDENTIFIER ::= { zxr10l2vpn 10} 
      zxr10l2vpnGlobalPara OBJECT IDENTIFIER ::= { zxr10l2vpnObjects 10}
      
      
L2vpnPWType ::= TEXTUAL-CONVENTION
        DISPLAY-HINT "d"
        STATUS       current
        DESCRIPTION
            "L2vpn PW Type. Assigned values are specified in 'IANA Allocations 
             for Pseudowire Edge to Edge Emulation (PWE3)"
        SYNTAX INTEGER {
		    unknown(0),
            fr-dlci-martini(1),
            aal5-sdu(2),
            atm-cell(3),
            vlan(4),
            ethernet(5),
            hdlc(6),
            ppp(7),
            sdh-cesom(8),
            atm-vccn(9),
            atm-vpcn(10),
            ip(11),
            atm-vcc1(12),
            atm-vpc1(13),
            aal5-pdu(14),
            fr-port(15),
            sdh-ceop(16),
            e1(17),
            t1-ds1(18),
            e3(19),
            t3-ds3(20),
            cesopsn(21),
            tdm-aal1(22),
            tdm-with-cas(23),
            tdm-aal2(24),
            fr-dlct(25)            
         }     
         
L2vpnVplsPolicy ::= TEXTUAL-CONVENTION
        DISPLAY-HINT "d"
        STATUS       current
        DESCRIPTION
            "L2vpn Vpls Policy."
        SYNTAX INTEGER {
		    tunnelling(1),
		    discarding(2),
		    terminating(3),
		    tunnellingMonitor(4)            
         }

