    MODULE-IDENTITY, OBJECT-TYPE,
    Integer32,IpAddress,Gauge32,
    Counter32,TimeTicks                                 FROM SNMPv2-SMI
    DisplayString,MacAddress,RowStatus                  FROM SNMPv2-TC
    OBJECT-GROUP,MODULE-COMPLIANCE                      FROM SNMPv2-CONF
    InetAddress,InetAddressType                         FROM INET-ADDRESS-MIB
    ZxAnPortList,VlanId                                 FROM ZTE-AN-TC-MIB
    ifIndex                                             FROM IF-MIB
    zxAnSubIfIndex                                      FROM ZTE-AN-IF-MIB
    zxAnMulticast                                       FROM ZTE-AN-SMI;

    zxAnIgmpMib MODULE-IDENTITY
        LAST-UPDATED    "201108241600Z"
        ORGANIZATION    "ZTE Corporation"
        CONTACT-INFO    "Weng Ningning  ZTE Corporation
                         Mail: wen.ningning@zte.com.cn
                         Tel : +86-21-68897487"
        DESCRIPTION     "This MIB defines Description of zte igmp service objects."
        REVISION        "201108241600Z"
        DESCRIPTION     "Initial version of this MIB module."
        ::= {zxAnMulticast 1}

    zxAnIgmpGlobalObjects      OBJECT IDENTIFIER ::= {zxAnIgmpMib 1}
    zxAnIgmpObjects            OBJECT IDENTIFIER ::= {zxAnIgmpMib 2}
    zxAnIgmpConformance        OBJECT IDENTIFIER ::= {zxAnIgmpMib 4}

    zxAnIgmpProtocolObjects    OBJECT IDENTIFIER ::= {zxAnIgmpObjects 1}
    zxAnIgmpMgmtObject         OBJECT IDENTIFIER ::= {zxAnIgmpObjects 2}
    zxAnIgmpVplsObjects        OBJECT IDENTIFIER ::= {zxAnIgmpObjects 3}
    zxAnIgmpLogObjects         OBJECT IDENTIFIER ::= {zxAnIgmpObjects 4}
    
    zxAnIgmpStatsObjects       OBJECT IDENTIFIER ::= {zxAnIgmpObjects 50}

    zxAnIgmpProtoBaseObjects   OBJECT IDENTIFIER ::= {zxAnIgmpProtocolObjects 2}
    zxAnIgmpSnoopingObjects    OBJECT IDENTIFIER ::= {zxAnIgmpProtocolObjects 3}
    zxAnIgmpProxyObjects       OBJECT IDENTIFIER ::= {zxAnIgmpProtocolObjects 4}

    zxAnIgmpProtoGlobalObjects 
                              OBJECT IDENTIFIER ::= {zxAnIgmpProtoBaseObjects 1}

    zxAnIgmpMgmtGlobalObject   OBJECT IDENTIFIER ::= {zxAnIgmpMgmtObject 1}
    
    zxAnIgmpLogGlobalObjects   OBJECT IDENTIFIER ::= {zxAnIgmpLogObjects 1 }

