-- 4. zxAnIgmpConformance
--------------------------------------------------------------------------------
zxAnIgmpCompliances      OBJECT IDENTIFIER ::= {zxAnIgmpConformance 1}
zxAnIgmpGroups           OBJECT IDENTIFIER ::= {zxAnIgmpConformance 2}

    zxAnIgmpCompliance MODULE-COMPLIANCE
        STATUS current
        DESCRIPTION
            "The compliance statement for entities which implement the
             ZTE-AN-IGMP-MIB."
        MODULE -- this module
            MANDATORY-GROUPS {
                zxAnIgmpProtocolGroup,
                zxAnIgmpManagementGroup,
                zxAnIgmpStatsGroup
            }
        ::= { zxAnIgmpCompliances 1 }

    zxAnIgmpProtocolGroup OBJECT-GROUP
        OBJECTS {
            zxAnIgmpEnable, zxAnIgmpIfAdminStatus,
            zxAnIgmpIfProtoVersion, zxAnIgmpIfFastLeaveEnable,
            zxAnIgmpMVlanIgmpAdminStatus, zxAnIgmpMVlanIgmpV1OperMode,
            zxAnIgmpMVlanIgmpV2OperMode, zxAnIgmpMVlanIgmpV3OperMode,
            zxAnIgmpMVlanIgmpWorkMode, zxAnIgmpMVlanIgmpSsmCtrlMode,
            zxAnIgmpMVlanIgmpSsmIpAddr, zxAnIgmpMVlanIgmpSsmIpAddrMask,
            zxAnIgmpSnoopingMVlanAgingTime, zxAnIgmpMVlanRobustnessVariable,
            zxAnIgmpMVlanHostVersion, zxAnIgmpMVlanHostIpAddr,
            zxAnIgmpMVlanUnsolReportInterval, zxAnIgmpMVlanHostQuerier,
            zxAnIgmpMVlanHostActualVersion, zxAnIgmpMVlanHostV1QuerierTimer,
            zxAnIgmpMVlanHostV2QuerierTimer, zxAnIgmpMVlanRouterIpAddr,
            zxAnIgmpMVlanQueryInterval, zxAnIgmpMVlanQueryRespInterval,
            zxAnIgmpMVlanLastMemberQryIntvl, zxAnIgmpMVlanLastMemberQryCount,
            zxAnIgmpMVlanStartUpQryInterval, zxAnIgmpMVlanStartUpQryCount,
