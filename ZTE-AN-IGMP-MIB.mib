------------------------------------------------------------------------------
    zxAnIgmpLogEnable OBJECT-TYPE
        SYNTAX      INTEGER {
            enabled(1),
            disabled(2)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The administrative status of IGMP log."
        DEFVAL { disabled }
        ::= { zxAnIgmpGlobalObjects 1 }
    
    zxAnIgmpRouterAlertEnable OBJECT-TYPE
        SYNTAX      INTEGER {
            enabled(1),
            disabled(2)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The administrative status of IGMP router alert option."
        DEFVAL { enabled }
        ::= { zxAnIgmpGlobalObjects 2 }
        
    zxAnIgmpBackwardCompatibilityEn OBJECT-TYPE
        SYNTAX      INTEGER {
            enabled(1),
            disabled(2)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The administrative status of IGMP backward compatibility."
        DEFVAL { disabled }
        ::= { zxAnIgmpGlobalObjects 3 } 
        
    zxAnIgmpSyslogReportingEnable OBJECT-TYPE
        SYNTAX      INTEGER {
            enabled(1),
            disabled(2)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The administrative status of IGMP syslog reporting."
        DEFVAL { disabled }
        ::= { zxAnIgmpGlobalObjects 4 } 

    zxAnIgmpCapabilities OBJECT-TYPE
        SYNTAX      BITS {
            supportMVlanPortListTable(0),
            notSupportIfBandwidth(1),
            supportMVlanPortListIITable(2)
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The IGMP compatibility.
             The bit value 1 means the function represented by this bit is 
             supported by this equipment.
             bit 0: value 1 means supporting zxAnIgmpMgmtMVlanPortListTable 
                    and not supporting zxAnIgmpMVlanPortListTable.
             bit 1: value 1 means not supporting zxAnIgmpIfBandwidth.
             bit 2: value 1 means supporting zxAnIgmpMgmtMVlanPortListIITable 
                    and not supporting zxAnIgmpMgmtMVlanPortListTable and 
                    zxAnIgmpMVlanPortListTable.
             NOTE: No recommended to display it in NMS." 
        ::= { zxAnIgmpGlobalObjects  100 }

------------------------------------------------------------------------------
-- 2. zxAnIgmpObjects
------------------------------------------------------------------------------

------------------------------------------------------------------------------
-- 2.1.2.1 zxAnIgmpProtoGlobalObjects
------------------------------------------------------------------------------
    zxAnIgmpEnable OBJECT-TYPE
        SYNTAX      INTEGER {
            enabled(1),
            disabled(2)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The administrative status of global IGMP service."
        DEFVAL { enabled }
        ::= { zxAnIgmpProtoGlobalObjects 1 }

------------------------------------------------------------------------------
-- 2.1.2.2 zxAnIgmpProtoIfTable
------------------------------------------------------------------------------

    zxAnIgmpProtoIfTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnIgmpProtoIfEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "A table that contains all IGMP protocol information of an user 
            interface."
        ::= { zxAnIgmpProtoBaseObjects 2 }

    zxAnIgmpProtoIfEntry OBJECT-TYPE
        SYNTAX      ZxAnIgmpProtoIfEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
           "A list of entries of zxAnIgmpProtoIfTable."
        INDEX { ifIndex, zxAnSubIfIndex }
        ::= { zxAnIgmpProtoIfTable 1 }

    ZxAnIgmpProtoIfEntry ::=
        SEQUENCE {
            zxAnIgmpIfAdminStatus          INTEGER,
            zxAnIgmpIfProtoVersion         INTEGER,
            zxAnIgmpIfFastLeaveEnable      INTEGER,
            zxAnIgmpIfBandwidth            Integer32,
            zxAnIgmpIfMaxGroups            Integer32
        }

    zxAnIgmpIfAdminStatus OBJECT-TYPE
        SYNTAX      INTEGER {
            enabled(1),
            drop(3),
            notSupport(255)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The administrative status of port's IGMP service. 
             The value 255 means the object is not supported."
        DEFVAL { enabled }
        ::= { zxAnIgmpProtoIfEntry 1 }

     zxAnIgmpIfProtoVersion OBJECT-TYPE
        SYNTAX      INTEGER {
                        igmpv1(1),
                        igmpv2(2),
                        igmpv3(3),
                        notSupport(255)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The max version of IGMP protocol which is supported on this port. 
             The value 255 means the object is not supported."
        DEFVAL { igmpv3 }
        ::= { zxAnIgmpProtoIfEntry 2 }

    zxAnIgmpIfFastLeaveEnable OBJECT-TYPE
        SYNTAX      INTEGER {
            enabled(1),
            disabled(2),
            notSupport(255)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The administrative status of port's fast-leave property. 
             The value 255 means the object is not supported."
        REFERENCE
            "TR-101 Page 13"
        DEFVAL { enabled }
        ::= { zxAnIgmpProtoIfEntry 3 }

    zxAnIgmpIfBandwidth OBJECT-TYPE
        SYNTAX      Integer32 (100..10240000|2147483647)
        UNITS       "kbps"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Multicast bandwidth of this port.
             Uplink port/PON port: The valid range is 10240~10240000 kbps, and 
             the default value is 1024000 kbps.
             VPort: The valid range is 100~204800 kbps, and the default value 
             is 102400 kbps.
             The value 2147483647 means the object is not supported."
        ::= { zxAnIgmpProtoIfEntry 4 }

    zxAnIgmpIfMaxGroups OBJECT-TYPE
        SYNTAX      Integer32 (0..8192|2147483647)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Maximum multicast groups that the port may join. 
             The value 0 means the port cannot join any groups. 
             The value 2147483647 means the object is not supported."
        DEFVAL { 8192 }
        ::= { zxAnIgmpProtoIfEntry 5 }

------------------------------------------------------------------------------
-- 2.1.2.3 zxAnIgmpProtoMVlanTable
------------------------------------------------------------------------------
    zxAnIgmpProtoMVlanTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnIgmpProtoMVlanEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "A table that contains IGMP protocol information in Multicast
            VLAN."
        ::= { zxAnIgmpProtoBaseObjects 3 }

    zxAnIgmpProtoMVlanEntry OBJECT-TYPE
        SYNTAX      ZxAnIgmpProtoMVlanEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "A list of entries of zxAnIgmpProtoMVlanTable."
        INDEX { zxAnIgmpMVid }
        ::= { zxAnIgmpProtoMVlanTable 1 }

    ZxAnIgmpProtoMVlanEntry ::=
        SEQUENCE {
            zxAnIgmpMVid                        Integer32,
            zxAnIgmpMVlanIgmpAdminStatus        INTEGER,
            zxAnIgmpMVlanIgmpV1OperMode         INTEGER,
            zxAnIgmpMVlanIgmpV2OperMode         INTEGER,
            zxAnIgmpMVlanIgmpV3OperMode         INTEGER,
            zxAnIgmpMVlanIgmpWorkMode           INTEGER,
            zxAnIgmpMVlanIgmpSsmCtrlMode        INTEGER,
            zxAnIgmpMVlanIgmpSsmIpAddr          IpAddress,
            zxAnIgmpMVlanIgmpSsmIpAddrMask      IpAddress,
            zxAnIgmpMVlanIgmpCVid               Integer32,
            zxAnIgmpMvlanIgmpMaxGroups          Integer32,
            zxAnIgmpMVlanGenQryPktTxGrpMode     INTEGER
        }

    zxAnIgmpMVid OBJECT-TYPE
        SYNTAX      Integer32(1..4094)
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "Multicast VLAN ID."
        ::= { zxAnIgmpProtoMVlanEntry 1 }

     zxAnIgmpMVlanIgmpAdminStatus OBJECT-TYPE
        SYNTAX      INTEGER {
            enabled(1),
            drop(3)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The administrative status of IGMP service in this MVLAN."
        DEFVAL { enabled }
        ::= { zxAnIgmpProtoMVlanEntry 2 }

    zxAnIgmpMVlanIgmpV1OperMode OBJECT-TYPE
        SYNTAX      INTEGER {
                        accept(1),
                        drop(3)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The operational mode of IGMP v1 packet."
        DEFVAL { accept }
        ::= { zxAnIgmpProtoMVlanEntry 3 }

    zxAnIgmpMVlanIgmpV2OperMode OBJECT-TYPE
        SYNTAX      INTEGER {
                        accept(1),
                        drop(3)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The operational mode of IGMP v2 packet."
        DEFVAL { accept }
        ::= { zxAnIgmpProtoMVlanEntry 4 }

    zxAnIgmpMVlanIgmpV3OperMode OBJECT-TYPE
        SYNTAX      INTEGER {
                        accept(1),
                        drop(3)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The operational mode of IGMP v3 packet."
        DEFVAL { accept }
        ::= { zxAnIgmpProtoMVlanEntry 5 }

     zxAnIgmpMVlanIgmpWorkMode OBJECT-TYPE
        SYNTAX      INTEGER {
                        snooping(1),
                        spr(2),
                        proxy(3),
                        router(4)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The IGMP work mode in this MVLAN. The meanings of the values
            are:
                snooping(1) - transparently forward IGMP packet, snooping
                    is defined in RFC 4541 and TR-101.
                spr(2) - transparently forward IGMP general query and
                    suppress IGMP report/leave for existing group membership,
                    SPR(Snooping with Proxy Reporting) is defined in TR-101.
                proxy(3) - act as a host on the V interface and act as a
                    router on the U interface, proxy is defined in RFC4605.
                router(4) - act as a terminal on the V interface and act as a
                    router on the U interface, router is defined in TR-101."
        REFERENCE
            "RFC 4541, RFC 4605, TR-101 Page 14"
        DEFVAL { proxy }
        ::= { zxAnIgmpProtoMVlanEntry 6 }

    zxAnIgmpMVlanIgmpSsmCtrlMode OBJECT-TYPE
        SYNTAX      INTEGER {
                        asm(1),
                        ssm(2),
                        asmAndSsm(3)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The ASM/SSM service control model. The meanings of the values
            are:
                asm(1) - only ASM(*, G) requests are permitted, ASM(Any-
                    Source Multicast) is defined in RFC 1112.
                ssm(2) - only SSM(S, G) requests are permitted, SSM(Source-
                    Specific Multicast) is defined in RFC 3569.
                asmAndSsm(3) --- both ASM and SSM requests are permitted."
        REFERENCE
            "RFC 1112, RFC 3569"
        DEFVAL { asmAndSsm }
        ::= { zxAnIgmpProtoMVlanEntry 7 }

    zxAnIgmpMVlanIgmpSsmIpAddr OBJECT-TYPE
        SYNTAX      IpAddress
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The SSM IP address in this MVLAN.
            All SSM(S, G) request must be in the same IP network segment as
            the configued SSM IP address and netmask."
        REFERENCE
            "RFC 4607 Section 4.3"
        DEFVAL { 'E8000000'H }
        ::= { zxAnIgmpProtoMVlanEntry 8 }

    zxAnIgmpMVlanIgmpSsmIpAddrMask OBJECT-TYPE
        SYNTAX      IpAddress
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The mask of SSM IP address in this MVLAN.
            All SSM(S, G) request must be in the same IP network segment as
            the configued SSM IP address and netmask."
        REFERENCE
            "RFC 4607 Section 4.3"
        DEFVAL { 'FF000000'H }
        ::= { zxAnIgmpProtoMVlanEntry 9 }
        
    zxAnIgmpMVlanIgmpCVid OBJECT-TYPE
        SYNTAX      Integer32(0..4094)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Multicast C-VID.
             Value 0 means no C-VID tag."
        DEFVAL { 0 }
        ::= { zxAnIgmpProtoMVlanEntry 10 }

    zxAnIgmpMvlanIgmpMaxGroups OBJECT-TYPE
        SYNTAX      Integer32 (0..8192)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Maximum multicast groups that the MVLAN may join. The value 0 means 
             the MVLAN cannot join any groups."
        DEFVAL { 8192 }
        ::= { zxAnIgmpProtoMVlanEntry 11 }

    zxAnIgmpMVlanGenQryPktTxGrpMode OBJECT-TYPE
        SYNTAX      INTEGER {
            usersInGroup(1),
            allUsers(2)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The general query packets transmission group mode only on pon
             interfaces."
        DEFVAL { usersInGroup }
        ::= { zxAnIgmpProtoMVlanEntry 12 }

------------------------------------------------------------------------------
-- 2.1.3.2 zxAnIgmpSnoopingMVlanTable
------------------------------------------------------------------------------
    zxAnIgmpSnoopingMVlanTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnIgmpSnoopingMVlanEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "A table that contains IGMP Snooping informations in multicast
            VLAN."
        ::= { zxAnIgmpSnoopingObjects 2 }

    zxAnIgmpSnoopingMVlanEntry OBJECT-TYPE
        SYNTAX      ZxAnIgmpSnoopingMVlanEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "A list of entries of zxAnIgmpSnoopingMVlanTable."
        INDEX { zxAnIgmpMVid }
        ::= { zxAnIgmpSnoopingMVlanTable 1 }

    ZxAnIgmpSnoopingMVlanEntry ::=
        SEQUENCE {
            zxAnIgmpSnoopingMVlanAgingTime      Integer32
        }

    zxAnIgmpSnoopingMVlanAgingTime OBJECT-TYPE
        SYNTAX      Integer32 (30..3600)
        UNITS       "seconds"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The timeout period in seconds for aging out multicast group
            memberships dynamically learned with IGMP which operates on
            a per interface per VLAN per multicast group basis.
            Note that it only takes effect in snooping mode."
        DEFVAL { 300 }
        ::= { zxAnIgmpSnoopingMVlanEntry 1 }

------------------------------------------------------------------------------
-- 2.1.4.2 zxAnIgmpProxyMVlanTable
------------------------------------------------------------------------------
    zxAnIgmpProxyMVlanTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnIgmpProxyMVlanEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "A table that contains IGMP proxy informations in Multicast
            VLAN."
        ::= { zxAnIgmpProxyObjects 2 }

    zxAnIgmpProxyMVlanEntry OBJECT-TYPE
        SYNTAX      ZxAnIgmpProxyMVlanEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "A list of entries of zxAnIgmpProxyMVlanTable."
        INDEX { zxAnIgmpMVid  }
        ::= { zxAnIgmpProxyMVlanTable 1 }

    ZxAnIgmpProxyMVlanEntry ::=
        SEQUENCE {
            zxAnIgmpMVlanRobustnessVariable     Integer32,
            zxAnIgmpMVlanHostVersion            INTEGER,
            zxAnIgmpMVlanHostIpAddr             IpAddress,
            zxAnIgmpMVlanUnsolReportInterval    Integer32,
            zxAnIgmpMVlanHostQuerier            IpAddress,
            zxAnIgmpMVlanHostActualVersion      INTEGER,
            zxAnIgmpMVlanHostV1QuerierTimer     TimeTicks,
            zxAnIgmpMVlanHostV2QuerierTimer     TimeTicks,
            zxAnIgmpMVlanRouterIpAddr           IpAddress,
            zxAnIgmpMVlanQueryInterval          Integer32,
            zxAnIgmpMVlanQueryRespInterval      Integer32,
            zxAnIgmpMVlanLastMemberQryIntvl     Integer32,
            zxAnIgmpMVlanLastMemberQryCount     Integer32,
            zxAnIgmpMVlanStartUpQryInterval     Integer32,
            zxAnIgmpMVlanStartUpQryCount        Integer32,
            zxAnIgmpMVlanIgmpPacketPriority     Integer32,
            zxAnIgmpMVlanQryPktProtoVersion     INTEGER,
            zxAnIgmpMVlanIgmpPktDscp            Integer32
        }

    zxAnIgmpMVlanRobustnessVariable OBJECT-TYPE
        SYNTAX      Integer32 (1..7)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "IGMP robustness variable in the MVLAN."
        REFERENCE
            "RFC 3376 Section 8.14.1"
        DEFVAL { 2 }
        ::= { zxAnIgmpProxyMVlanEntry 1 }

     zxAnIgmpMVlanHostVersion OBJECT-TYPE
        SYNTAX      INTEGER {
                        auto(1),
                        igmpv2(2),
                        igmpv3(3)
                    }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The administrative version of IGMP packet sent on the host
            interface.
                auto(1) - IGMP host version is dynamicly determined by
                    the attached router version, according to IGMP Host
                    Compatibility Mode described in RFC 3376.
                igmpv2(2) - IGMP host version is IGMP v2.
                igmpv3(3) - IGMP host version is IGMP v3."
        REFERENCE
            "RFC 3376 Section 7.2.1"
        DEFVAL { auto }
        ::= { zxAnIgmpProxyMVlanEntry 2 }

     zxAnIgmpMVlanHostIpAddr OBJECT-TYPE
        SYNTAX      IpAddress
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The source IP address of IGMP packet sent on the host
            interface."
        DEFVAL { 'C0A8020E'H }
        ::= { zxAnIgmpProxyMVlanEntry 3 }

    zxAnIgmpMVlanUnsolReportInterval OBJECT-TYPE
        SYNTAX      Integer32 (1..60)
        UNITS       "seconds"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "IGMP unsolicited report interval in the MVLAN."
        REFERENCE
            "RFC 3376 Section 8.11"
        DEFVAL { 1 }
        ::= { zxAnIgmpProxyMVlanEntry 4 }

    zxAnIgmpMVlanHostQuerier OBJECT-TYPE
        SYNTAX      IpAddress
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The IP address of the IGMP Querier attached on the host
            interface which is learned from received IGMP general query."
        REFERENCE
            "RFC 3376"
        ::= { zxAnIgmpProxyMVlanEntry 5 }

    zxAnIgmpMVlanHostActualVersion OBJECT-TYPE
        SYNTAX      INTEGER {
                        igmpv1(1),
                        igmpv2(2),
                        igmpv3(3)
                    }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The running version of IGMP packet sent on the host interface."
        REFERENCE
            "RFC 3376"
        DEFVAL { igmpv3 }
        ::= { zxAnIgmpProxyMVlanEntry 6 }

    zxAnIgmpMVlanHostV1QuerierTimer OBJECT-TYPE
        SYNTAX      TimeTicks
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The time remaining until the host assumes that there are
            no IGMPv1 routers present on the interface.  While this is
            non-zero, the host will reply to all queries with version 1
            membership reports.  This variable applies to IGMPv2 or v3
            hosts that are forced to run in v1 for compatibility with
            v1 routers present on the host interface."
        REFERENCE
            "RFC 3376 Section 7.2.1"
        DEFVAL { 0 }
        ::= { zxAnIgmpProxyMVlanEntry 7 }

    zxAnIgmpMVlanHostV2QuerierTimer OBJECT-TYPE
        SYNTAX      TimeTicks
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The time remaining until the host assumes that there are
            no IGMPv2 routers present on the interface.  While this is
            non-zero, the host will reply to all queries with version 1
            or 2 membership reports.  This variable applies to IGMPv3
            hosts that are forced to run in v2 for compatibility with
            v2 routers present on the host interface."
        REFERENCE
            "RFC 3376 Section 7.2.1"
        DEFVAL { 0 }
        ::= { zxAnIgmpProxyMVlanEntry 8 }

     zxAnIgmpMVlanRouterIpAddr OBJECT-TYPE
        SYNTAX      IpAddress
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The source IP address of IGMP query packet sent on the router
            interface."
        DEFVAL { 'C0A8020E'H }
        ::= { zxAnIgmpProxyMVlanEntry 9 }

    zxAnIgmpMVlanQueryInterval OBJECT-TYPE
        SYNTAX      Integer32 (30..300)
        UNITS       "seconds"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "IGMP general query interval in the MVLAN."
        REFERENCE
            "RFC 3376 Section 8.2"
        DEFVAL { 125 }
        ::= { zxAnIgmpProxyMVlanEntry 10 }

    zxAnIgmpMVlanQueryRespInterval OBJECT-TYPE
        SYNTAX      Integer32 (10..250)
        UNITS       "0.1second"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "IGMP query max response time in the MVLAN."
        REFERENCE
            "RFC 3376 Section 8.3"
        DEFVAL { 100 }
        ::= { zxAnIgmpProxyMVlanEntry 11 }

    zxAnIgmpMVlanLastMemberQryIntvl OBJECT-TYPE
        SYNTAX      Integer32 (1..255)
        UNITS       "0.1second"
        MAX-ACCESS  read-write
        STATUS current
        DESCRIPTION
            "IGMP last member query interval in the MVLAN."
        REFERENCE
            "RFC 3376 Section 8.8"
        DEFVAL { 10 }
        ::= { zxAnIgmpProxyMVlanEntry 12 }

    zxAnIgmpMVlanLastMemberQryCount OBJECT-TYPE
