--------------------------------------------------------------------------------
--  4.1 MAC Address Learn Limit Table
--------------------------------------------------------------------------------
    
    zxAnSecIfMacLearnedLimitTable   OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnSecIfMacLearnedLimitEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION 
            "This table includes the ethernet-like interface configuration."
        ::= { zxAnSecMacSecurityObjects 2 }

    zxAnSecIfMacLearnedLimitEntry   OBJECT-TYPE
        SYNTAX      ZxAnSecIfMacLearnedLimitEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION 
            "An entry in zxAnSecIfSecurityConfTable."
        INDEX       { ifIndex, zxAnSubIfIndex  }
        ::= { zxAnSecIfMacLearnedLimitTable 1 }

    ZxAnSecIfMacLearnedLimitEntry  ::=  SEQUENCE {        
        zxAnSecIfMacLearnedLimitEnable   INTEGER,
        zxAnSecIfMaxLearnedMacs          Integer32
        }

    zxAnSecIfMacLearnedLimitEnable OBJECT-TYPE 
        SYNTAX      INTEGER 
        {
            enable(1),
            disable(2)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION     
            "Enable/disable MAC learning limit."
        DEFVAL  { enable }              
        ::= { zxAnSecIfMacLearnedLimitEntry 3 }

    zxAnSecIfMaxLearnedMacs OBJECT-TYPE 
        SYNTAX      Integer32(0..1048576 | 2147483647) 
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION     
            "Configure the number of the MAC address learnt. One port can only
             learn a maximum of the MACs configured.
             When the value of bit 1 (supportMaxLearnedMacsRangeTo1M) of 
             zxAnSecInterfaceCapabilities is 1, the valid range of the object is
             from 0 to 1048576 or 2147483647, the value 2147483647 means no 
             limit, and the default value is 2147483647, otherwise the valid 
             range is from 0 to 16383 or 65535, the value 65535 means no limit, 
             and the default value is 65535."
        DEFVAL  { 2147483647 }
        ::= { zxAnSecIfMacLearnedLimitEntry 4 }

