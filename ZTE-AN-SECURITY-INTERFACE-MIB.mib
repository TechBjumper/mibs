-- 2.2 Storm Control Interface Table
------------------------------------------------------------------------------ 

    StormCtrlPktType ::= TEXTUAL-CONVENTION
        STATUS      current
        DESCRIPTION     
            "Storm control packet type."
        SYNTAX      INTEGER
        {
            broadcast(1),
            unknownUnicast(2),
            multicast(3) 
        }
    
    zxAnSecStormCtrlIfTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnSecStormCtrlIfEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION 
            "Interface packet rate limit information table."
        ::= { zxAnSecStormCtrlObjects 3 }
         
     zxAnSecStormCtrlIfEntry OBJECT-TYPE
         SYNTAX      ZxAnSecStormCtrlIfEntry
         MAX-ACCESS  not-accessible
         STATUS      current
         DESCRIPTION
             "A zxAnSecStormCtrlIfTable entry."
         INDEX  
         {
             ifIndex, 
             zxAnSubIfIndex,
             zxAnSecStormCtrlIfPktType
         }
         ::= { zxAnSecStormCtrlIfTable 1 }
         
     ZxAnSecStormCtrlIfEntry ::= SEQUENCE {
         zxAnSecStormCtrlIfPktType            StormCtrlPktType,
         zxAnIfStormCtrlRateLmtUnit           INTEGER,
         zxAnIfStormCtrlRateLmtIngress        Integer32,
         zxAnIfStormCtrlRateLmtEgress         Integer32
         }

    zxAnSecStormCtrlIfPktType  OBJECT-TYPE 
        SYNTAX      StormCtrlPktType 
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "Storm control packet type."
        ::= { zxAnSecStormCtrlIfEntry 1 }
        
    zxAnIfStormCtrlRateLmtUnit OBJECT-TYPE
        SYNTAX      INTEGER 
        {
            pps(1),
            kbps(2)
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The unit of packet rate limit. Option 'kbps' has been deprecated."
        ::= { zxAnSecStormCtrlIfEntry 2 } 

    zxAnIfStormCtrlRateLmtIngress OBJECT-TYPE 
        SYNTAX      Integer32(0..1000000|2147483646|2147483647)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Ingress packet rate limit. 
             Value 0 means that the packet will be discarded.
             0x7FFFFFFE means no limit.                            
             0x7FFFFFFF means not supported."               
        DEFVAL  { 100 }
        ::= { zxAnSecStormCtrlIfEntry 3 }
        
    zxAnIfStormCtrlRateLmtEgress OBJECT-TYPE 
        SYNTAX      Integer32(0..1000000|2147483646|2147483647) 
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Egress packet rate limit. 
             Value 0 means that the packet will be discarded.
             0x7FFFFFFE means no limit.
             0x7FFFFFFF means not supported."              
        DEFVAL  { 100 }
        ::= { zxAnSecStormCtrlIfEntry 4 }

