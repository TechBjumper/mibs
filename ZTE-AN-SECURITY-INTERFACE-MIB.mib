    zxAnSecStormCtrlGlobalObjects      
                             OBJECT IDENTIFIER ::= { zxAnSecStormCtrlObjects 1 }
    
    zxAnSecStormCtrlBcastRateLimit  OBJECT-TYPE 
        SYNTAX      Integer32(0..1000000|2147483647) 
        UNITS       "pps"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION 
            "Broadcast packet rate limit.
             Value 0 means that the packet will be discarded.
             0x7FFFFFFF means no limit."
        DEFVAL  { 100 }                
        ::= { zxAnSecStormCtrlGlobalObjects 1 }
                              
    zxAnSecStormCtrlMcastRateLimit OBJECT-TYPE 
        SYNTAX      Integer32(0..1000000|2147483647) 
        UNITS       "pps"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION 
            "Multicast packet rate limit.
             Value 0 means that the packet will be discarded.
             0x7FFFFFFF means no limit."
        DEFVAL  { 100 }                
        ::= { zxAnSecStormCtrlGlobalObjects 2 }

    zxAnStormCtrlUnknownUcastRateLmt OBJECT-TYPE 
        SYNTAX      Integer32(0..1000000|2147483647) 
        UNITS       "pps"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION 
            "Unknown unicast packet rate limit.
             Value 0 means that the packet will be discarded.
             0x7FFFFFFF means no limit."
        DEFVAL  { 100 }                
        ::= { zxAnSecStormCtrlGlobalObjects 3 }
        
    zxAnSecStormCtrlBcastBurstSize  OBJECT-TYPE 
