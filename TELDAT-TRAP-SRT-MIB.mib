    ::= { srtNotificationsPrefix 10 }

srtNotification11 NOTIFICATION-TYPE
    OBJECTS { 
              srt11-source-mac, 
              srt11-dest-mac,
              srt11-port,
              srt11-interface,
              srt11-circuit-name 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event SRT.11

        Message: 'Frame from $1 to $2 dropped, input port $3 interface $4$5 not in forwarding state'

        Parameters list:
              $1: srt11-source-mac
              $2: srt11-dest-mac
              $3: srt11-port
              $4: srt11-interface
              $5: srt11-circuit-name

        A MAC frame was received on a port that is still only in 'learning'
        state. Frames are only bridged when the input port is in 'forwarding'
        state. While the port is still in 'learning' state, they are only
        processed to learn the source addresses for the filtering database.
        The frame will not be bridged."
    ::= { srtNotificationsPrefix 11 }

