    ::= { srtNotificationsPrefix 7 }

srtNotification8 NOTIFICATION-TYPE
    OBJECTS { 
              srt8-source-mac, 
              srt8-dest-mac,
              srt8-reformatted-length,
              srt8-output-maximum,
              srt8-port,
              srt8-interface,
              srt8-circuit-name 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event SRT.8

        Message: 'Frame from $1 to $2 is too big (reformatted length $3 bytes > output maximum size $4 bytes) for port $5 interface $6$7, dropped'

        Parameters list:
              $1: srt8-source-mac
              $2: srt8-dest-mac
              $3: srt8-reformatted-length
              $4: srt8-output-maximum
              $5: srt8-port
              $6: srt8-interface
              $7: srt8-circuit-name

        The specified frame is too large to send on this outgoing port and
        interface. The reformatted-length is the size of the frame including
        MAC headers after any mapping of data link headers."
    ::= { srtNotificationsPrefix 8 }

srtNotification9 NOTIFICATION-TYPE
    OBJECTS { 
              srt9-source-mac, 
              srt9-dest-mac,
              srt9-interface 
            }
    STATUS current
    DESCRIPTION
