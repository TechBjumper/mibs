            sdCard(2)               
            }        
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION     "The type of the device."                        
        ::=  {  zxAnFileSyncStatusEntry  5  }

-------------------------------------------------------------------------------
-- 4. File Explorer Objects
-- 4.1 File Explorer Global Objects
-------------------------------------------------------------------------------
    zxAnFileExplorerSourceFile     OBJECT-TYPE 
        SYNTAX      DisplayString (SIZE (1..128))
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The source file.
             The format is: 'rack/shelf/slot/devType/devId/directory/file'."
        ::=  {  zxAnFileExplorerGlobalObjects  1  }

    zxAnFileExplorerDestinationDir  OBJECT-TYPE
        SYNTAX      DisplayString (SIZE (0..128))
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The destination directory.
             The format is: 'rack/shelf/slot/devType/devId/directory'.
             If the zxAnFileExplorerOperation is 'delete' or 'rename', this 
             object will be set to empty string."
        ::= { zxAnFileExplorerGlobalObjects 2 }

    zxAnFileExplorerRenamedName  OBJECT-TYPE
        SYNTAX      DisplayString (SIZE (0..128))
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The renamed name.
             If the zxAnFileExplorerOperation is 'delete', this object will be 
             set to empty string.
             If the zxAnFileExplorerOperation is 'copy' and this object is not 
             empty string, the source file name will be changed to the 
             zxAnFileExplorerRenamedName after the copying operation.
             If the zxAnFileExplorerOperation is 'copy' and this object is empty 
             string, the source file name will not be changed after the copy 
             operation.
             If the zxAnFileExplorerOperation is 'rename', this object means the 
             renamed name."
        ::= { zxAnFileExplorerGlobalObjects 3 }
        
    zxAnFileExplorerOperation  OBJECT-TYPE
        SYNTAX      INTEGER {
            delete(1),
            copy(2),
            cancelCopy(3),
            rename(4)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The file operation type."
        ::= { zxAnFileExplorerGlobalObjects 4 }

     zxAnFileExplorerOperStatus  OBJECT-TYPE
        SYNTAX      INTEGER {
            notStarted(1),
            inProgress(2),
            success(3),
            failed(4)
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The status of the file operation."
        ::= { zxAnFileExplorerGlobalObjects 5 }

     zxAnFileExplorerOperProgress  OBJECT-TYPE
        SYNTAX      Integer32(0..100)
        UNITS       "percent"
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The progress of the file operation."
        ::= { zxAnFileExplorerGlobalObjects 6 }

     zxAnFileExplorerOperFailedReason  OBJECT-TYPE
        SYNTAX      INTEGER {
            noError(1),
            fileNotExisted(2),
            fileExisted(3),
            diskFull(4),
            timeout(5),
            otherOperationInProgress(6),
            systemFileNameUsed(7),
            slaveControlCardNotReady(8),
            dataTransmissionFailed(9),
            otherErrors(255)
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The failed reasion of the file operation."
        ::= { zxAnFileExplorerGlobalObjects 7 }

-------------------------------------------------------------------------------
-- 4.2 File List Table
-------------------------------------------------------------------------------
    zxAnFileListTable   OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnFileListEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION 
            "The file list table.
             This table is added to support the new index of the NE files: 
             'rack-shelf-slot-devType-devId-directory-name'."
        ::= { zxAnFileExplorerObjects  2 }

    zxAnFileListEntry   OBJECT-TYPE
        SYNTAX      ZxAnFileListEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "An entry of zxAnFileListTable. "
        INDEX  { zxAnFileListRack, 
                 zxAnFileListShelf,
                 zxAnFileListSlot,
                 zxAnFileListDevType,
                 zxAnFileListDevId,
                 zxAnFileListDirectory,
                 zxAnFileListName
               }
        ::=  { zxAnFileListTable 1 }

     ZxAnFileListEntry ::= SEQUENCE {
        zxAnFileListRack                Integer32,
        zxAnFileListShelf               Integer32,
        zxAnFileListSlot                Integer32,
        zxAnFileListDevType             INTEGER,
        zxAnFileListDevId               Integer32,
        zxAnFileListDirectory           DisplayString,
        zxAnFileListName                DisplayString,
        zxAnFileListSize                Integer32,
        zxAnFileListWriteTime           DateAndTime,
        zxAnFileListPermissions         BITS
        }

     zxAnFileListRack  OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The rack number."
        ::= { zxAnFileListEntry 1 }

     zxAnFileListShelf  OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The shelf number."
        ::= { zxAnFileListEntry 2 }

     zxAnFileListSlot  OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The slot number."
        ::= { zxAnFileListEntry 3 }

     zxAnFileListDevType  OBJECT-TYPE
        SYNTAX      INTEGER {
            flash(1),
            sdCard(2)
        }
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The device type.
             1 means flash;
             2 means SD card."
        DEFVAL  { flash }
        ::= { zxAnFileListEntry 4 }

     zxAnFileListDevId  OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The device ID. 
             This object is not used now. "
        DEFVAL  { 1 }
        ::= { zxAnFileListEntry 5 }
        
     zxAnFileListDirectory  OBJECT-TYPE
        SYNTAX      DisplayString (SIZE (1..128))
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The full directory of the file."
        ::= { zxAnFileListEntry 6 }
        
     zxAnFileListName  OBJECT-TYPE
        SYNTAX      DisplayString (SIZE (1..128))
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The file name. "
        ::= { zxAnFileListEntry 7 }

     zxAnFileListSize  OBJECT-TYPE
        SYNTAX      Integer32
        UNITS       "bytes"
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The file size."
        ::= { zxAnFileListEntry 8 }

     zxAnFileListWriteTime  OBJECT-TYPE
        SYNTAX      DateAndTime
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The write time of the file."
        ::= { zxAnFileListEntry 9 }

     zxAnFileListPermissions  OBJECT-TYPE
        SYNTAX      BITS{
            read(0),
            write(1)
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The access permissions of the file."
        ::= { zxAnFileListEntry 10 }

