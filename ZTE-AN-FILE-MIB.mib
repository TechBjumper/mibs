-- 31. zxAnSwConformance
-------------------------------------------------------------------------------
    zxAnFileCompliance MODULE-COMPLIANCE
        STATUS current
        DESCRIPTION 
            "The compliance statement for SNMP entities which implement the
            ZTE-AN-SOFTWARE-MIB."
        MODULE -- this module
        
            MANDATORY-GROUPS { 
                  zxAnFileLoadDefConfGroup,            
                  zxAnFileFtpOperGroup,
                  zxAnFileMgmtGroup,
                  zxAnFileSyncGroup,                  
                  zxAnFileNotificationsGroup
                }
        ::= { zxAnFileCompliances 1 }

    zxAnFileLoadDefConfGroup OBJECT-GROUP
        OBJECTS { 
                  zxAnFileLoadDefaultConfiguration
                }
        STATUS current
        DESCRIPTION 
            "The collection of objects which are used to to load default 
            Configuration."
             
        ::= { zxAnFileGroups 1 }
                                                                             
    zxAnFileFtpOperGroup OBJECT-GROUP
        OBJECTS { zxAnFileFtpFileType, zxAnFileFtpOperType, zxAnFileFtpAction,
                  zxAnFileFtpSvrIpAddressType,zxAnFileFtpSvrIpAddress, 
                  zxAnFileFtpSvrUserName, zxAnFileFtpSvrUserPwd,
                  zxAnFileFtpSvrProtocolType, zxAnFileFtpSvrFilePath,
                  zxAnFileFtpSvrFileName, zxAnFileFtpPerfLogType,
                  zxAnFileFtpFileRetrieveStartTime, 
                  zxAnFileFtpFileRetrieveEndTime,
                  zxAnFileFtpOperStatus, zxAnFileFtpCurrFileName,
                  zxAnFileFtpCurrFileSize,zxAnFileFtpCurrFileProgress,
                  zxAnFileFtpTotalFiles, zxAnFileFtpSuccessFiles,
                  zxAnFileFtpFailedReason
                }
        STATUS current
        DESCRIPTION 
            "The collection of objects which are used to implement transmission
             of file between an agent and a manager (download & upload)."

        ::= { zxAnFileGroups 2 }

    zxAnFileMgmtGroup OBJECT-GROUP
        OBJECTS { 
                   zxAnFileSize, zxAnFileModifyTime, 
                   zxAnFileStorageDevType, zxAnFileOperation
                }
        STATUS current
        DESCRIPTION 
            "The collection of objects which are used to implement file 
            management such as display attribute of file, delete file, 
            synchronize file to slave board."
             
        ::= { zxAnFileGroups 3 }

    zxAnFileSyncGroup OBJECT-GROUP
        OBJECTS { 
                   zxAnFileSyncRack, zxAnFileSyncShelf,
                   zxAnFileSyncFileType,zxAnFileSyncFileName,
                   zxAnFileSyncOperation, zxAnFileSyncStatus, 
                   zxAnFileSyncProgress,  zxAnFileSyncFailedReason,
                   zxAnFileSyncDevType
                }
        STATUS current
        DESCRIPTION 
            "The collection of objects which are used to implement file
             synchronization."
             
        ::= { zxAnFileGroups 4 }
        
    zxAnFileNotificationsGroup OBJECT-GROUP
        OBJECTS {  
                   zxAnFileFtpDownloadFinished,
                   zxAnFileFtpUploadFinished
                }
        STATUS current
        DESCRIPTION 
            "The notifications which indicate specific changes, 
            include file upload/download finished."

        ::= { zxAnFileGroups 10 } 
        
