        zxAnPonOltPerfFallWarnThresh          HCPerfCurrentCount,
        zxAnPonOltPerfFallWarnClrThresh       HCPerfCurrentCount,
        zxAnPonOltPerfFallAlmThresh           HCPerfCurrentCount,
        zxAnPonOltPerfFallAlmClrThresh        HCPerfCurrentCount,
        zxAnPonOltPerfThreshUsed              BITS,
        zxAnPonOltPerfAlmConfRowStatus        RowStatus
        }

